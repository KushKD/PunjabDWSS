	<queryString>
		<![CDATA[SELECT	zone_id,zone_name,	circle_id,circle_name,	district_id,district_name,
	divisional_id,divisional_name,	count(village_name) as village_name ,sum(sew_connection) as sew_connection
	FROM	(
SELECT	z.zone_id,zone_name,	c.circle_id,circle_name,	mv.district_id,district_name,
	di.divisional_id,divisional_name,	zz.village_id,village_name||'('||zz.village_id||')' as village_name,	sew_connection
FROM	(select zz.* from(SELECT	village_id,	sew_connection
FROM	(SELECT	village_id,	sew_connection,	rank () over(partition by village_id order by sewerage_tariff_id desc)
from	prwss_main.mst_village_sewerage_tariff
WHERE	status<>'D')zzz	WHERE rank=1)ZZ
inner join
(SELECT	sh.scheme_id, scheme_source, mv.village_id,scheme_commissioned_date
FROM	prwss_main.mst_scheme_header sh, prwss_main.mst_scheme_village sv, prwss_main.mst_village mv
WHERE	sh.status <> 'D' AND sv.status <> 'D' AND mv.status <> 'D' AND sv.village_id = mv.village_id
AND sh.scheme_id = sv.scheme_id AND sh.scheme_upgraded = sv.scheme_upgraded
AND sv.scheme_commissioned_date IS NOT NULL AND mv.spl_flags IS NULL   AND scheme_source = 'SEWERAGE'
GROUP BY sh.scheme_id,scheme_source,scheme_commissioned_date, mv.village_id) yy
on zz.village_id=yy.village_id)zz,	prwss_main.mst_village mv,
	prwss_main.mst_zone z,			prwss_main.mst_circle c,
	prwss_main.mst_district d,		prwss_main.mst_divisional di
WHERE	zz.village_id=mv.village_id 		and mv.divisional_office_id=di.divisional_id
	and mv.district_id=d.district_id	and d.circle_id=c.circle_id
	and c.zone_id =z.zone_id)main where $P!{where}
group by zone_id,zone_name,	circle_id,circle_name,	district_id,district_name,
	divisional_id,divisional_name
ORDER BY zone_name,circle_name,district_name,divisional_name,village_name]]>
	</queryString>
