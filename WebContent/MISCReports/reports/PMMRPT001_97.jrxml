	<queryString>
		<![CDATA[select zone_name,circle_name,district_name,count(village_id)vill_name, sum(water_conn)no_of_water_connection from
(select x5.*, case when (wc_date_curr >= admin_app_gop_letter_date) then (wc_curr-wc_pre) else 0 end as water_conn from



(select x4.*, wc_curr, wc_date_curr

 from
(select * from
(select distinct zone_name,circle_name,district_name,scheme_id,  scheme_name, prog_id, scheme_operated_by,village_id,vill_name,habitation_type,admin_app_gop_letter_date,scheme_commissioned_date,village_category, scheme_upgraded, wc_pre, wc_pre_date, --, wc_date_curr,case when wc_date_curr is null then 0 else wc_curr end as wc_curr
dense_rank() over (partition by village_id order by wc_pre_date desc nulls last, wc_pre desc) r
from

(select zone_name,circle_name,district_name,scheme_id,  scheme_name, prog_id, scheme_operated_by,t.village_id,vill_name,habitation_type,admin_app_gop_letter_date,scheme_commissioned_date,village_category, scheme_upgraded, wc_pre,  --, wc_date_curr,case when wc_date_curr is null then 0 else wc_curr end as wc_curr
case when  wc_date_pre < admin_app_gop_letter_date then wc_date_pre else null end wc_pre_date
 from
(select zone_name,circle_name,district_name,scheme_id,  scheme_name, prog_id, scheme_operated_by,village_id,vill_name,habitation_type,admin_app_gop_letter_date,scheme_commissioned_date, scheme_upgraded, village_category
 from

(select zone_name,circle_name,district_name, sch.*
 from
(select distinct mv.district_id,location_id,mv.block_id,sh.scheme_id , scheme_name, prog_id, scheme_operated_by,sv.village_id,vill_name,habitation_type,scheme_commissioned_date, sv.scheme_upgraded,sh.scheme_source,village_category,admin_app_gop_letter_date,
rank() over (partition by sv.village_id order by scheme_commissioned_date desc nulls last, sv.scheme_id) sch_r

from prwss_main.mst_scheme_village sv, prwss_main.mst_scheme_header sh,prwss_main.mst_village mv, prwss_main.mst_scheme_details sd

where sh.scheme_id = sv.scheme_id
and sv.village_id = mv.village_id
and sh.scheme_id = sd.scheme_id
and sh.scheme_upgraded = sd.scheme_upgraded
and sh.scheme_upgraded = sv.scheme_upgraded
and sh.scheme_source not in ('SEWERAGE','TUBEWELLWITHRO','HANDPUMP')
and scheme_commissioned_date is not null
and prog_id = 'PROG30'
and sh.status <> 'D' and sv.status <> 'D' and mv.status <> 'D' and spl_flags is null) sch, prwss_main.mst_district md, prwss_main.mst_circle c, prwss_main.mst_zone z
where sch.district_id = md.district_id
and md.circle_id=c.circle_id and c.zone_id=z.zone_id
) x
where sch_r = 1 ) t

left join

(select village_id, wc_pre, wc_date_pre from
(select village_id, (no_of_water_connection) as wc_pre, as_on_date as wc_date_pre , rank() over (partition by village_id order by as_on_date desc nulls last,connection_id) wc_rank
	from prwss_main.mst_village_connection where status <> 'D') wc ) wc_pre
	on t.village_id = wc_pre.village_id) x2 ) x3 where r = 1)x4
left join
	(select village_id, wc_curr, wc_date_curr from
(select village_id, (no_of_water_connection) as wc_curr, as_on_date as wc_date_curr , rank() over (partition by village_id order by as_on_date desc nulls last,connection_id) wc_rank
	from prwss_main.mst_village_connection where status <> 'D') wc 	where
	wc_rank = 1 ) wc_curr

	on x4.village_id = wc_curr.village_id
	order by circle_name,  scheme_name, vill_name) x5 order by circle_name, scheme_name, vill_name

) main2
where $P!{where}
group by zone_name,circle_name,district_name]]>
	</queryString>
