	<queryString>
		<![CDATA[select zone_id,zone_name,circle_name,circle_id,district_name,district_id,divisional_name,divisional_id,scheme_id,prog_id,village_id,village_name,block_id,block_name,
	constituency_id,constituency_name,target_commissioning_date,
	balance/100000 balance,rel_to_office,row_sch,estimate_cost/100000 estimate_cost,'--' as status_of_revised,scheme_source, 0 as funds_available,prog_name,scheme_name,exp/100000 exp,


	case when habitation_type='MH' and ncpc_2008='NC' then vill else null end as nc_mh,
	case when habitation_type='OH' and ncpc_2008='NC' then vill else null end as nc_oh,
	case when habitation_type='MH' and ncpc_2008='PC' then vill else null end as pc_mh,
	case when habitation_type='OH' and ncpc_2008='PC' then vill else null end as pc_oh,
	case when habitation_type='MH' and ncpc_2008='FC' and scheme_commissioned_date>'2008-04-01' then vill else null end as slip_mh,
	case when habitation_type='OH' and ncpc_2008='FC' and scheme_commissioned_date>'2008-04-01' then vill else null end as slip_oh,
	case when habitation_type='MH' then vill else null end mh,
	case when habitation_type='OH' then vill else null end oh



from
(select z.zone_id,z.zone_name,c.circle_id,c.circle_name,d.district_id,district_name,divisional_id,divisional_name,block_name,constituency_name,main.*,
	(admin_app_gop_amount-exp) as balance

from
(select xx.*,case when xxx.expenditure is null then 0 else xxx.expenditure end as exp,estimate_cost,rel_to_office,target_commissioning_date
from


(select *,rank() over (partition by scheme_id order by village_id,location_id)as row_sch
from
(select sh.location_id,sh.scheme_id,sh.prog_id,sh.scheme_source,sh.scheme_name,mv.village_id,village_name,habitation_type,nc_pc_status,scheme_commissioned_date,sd.scheme_completed_date,block_id,constituency_id,
	case when admin_app_gop_amount is null then 0 else admin_app_gop_amount end as admin_app_gop_amount,admin_app_gop_letter_date,admin_app_gop_letter_no,vill,ncpc_2008,prog_name,
	rank() over (partition by mv.village_id order by scheme_commissioned_date desc,sh.scheme_id desc,admin_app_gop_letter_date desc,sh.scheme_upgraded)rank
from prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_details sd,prwss_main.mst_village mv,
	-----------------------------------------------------current nc pc status--------------------------------------
	(select village_id,(village_name||'('||village_id ||')' ) vill, nc_pc_status, ncpc_2008, prog_name
		from
		(select x.*,scheme_id,scheme_commissioned_date,prog_id,
		case when (ncpc_2008 in ('NC','PC') and scheme_commissioned_date is not null and scheme_commissioned_date>'2008-04-01') or spl_flags is not null then 'FC' else ncpc_2008 end as nc_pc_status
		from
		(select mv.*,nc_pc_status as ncpc_2008, proposed_program_name prog_name
		from prwss_main.mst_village mv,prwss_main.tbl_proposed_program_2008 ncpc
		where mv.village_id=ncpc.village_id
		and   mv.status<>'D')x
		left join
		(select * from
		(select mv.village_id,sh.scheme_id,scheme_commissioned_date,prog_id,
		rank() over (partition by mv.village_id order by scheme_commissioned_date desc, sh.scheme_id desc,admin_app_gop_letter_date desc,sh.scheme_upgraded desc) row_id
		from prwss_main.mst_village mv,prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd
		where mv.village_id=sv.village_id
		--and   sh.scheme_source not in ( 'SEWERAGE','IMPROVEMENT')
		and   sh.scheme_id=sv.scheme_id and sv.scheme_id=sd.scheme_id and sh.status<>'D' and sd.status<>'D'
		and   mv.status<>'D' and sv.status<>'D'
		and   scheme_commissioned_date>=to_date('2008-04-01','yyyy-mm-dd') and scheme_commissioned_date < now())y where row_id=1)xx
		on x.village_id=xx.village_id)xx) a,
--------------------------------------------------------------------------------------------------------------------
	(select *
	from
	(select  th.tender_id,scheme_code,(scheme_name||' ('||scheme_code||')') sch_name,open_date,publish_date,
		rank() over (partition by scheme_code order by signing_of_contract,open_date,publish_date,th.tender_id) rank,signing_of_contract
	from prwss_main.t_tender_hdr th,prwss_main.mst_scheme_header sh,prwss_main.t_tender_award ta
	where th.scheme_code=sh.scheme_id
	and   th.tender_id=ta.tender_id
	and tender_type='WORKS' and signing_of_contract is not null
	and   th.status<>'D' and sh.status<>'D' and ta.status<>'D')a
	where rank=1
	)b
where sh.scheme_id=sv.scheme_id
and   sv.scheme_id=sd.scheme_id
and   sd.scheme_id=b.scheme_code
and   sv.village_id=mv.village_id
and   mv.village_id=a.village_id
and   sh.status<>'D' and sv.status<>'D'
and sd.status<>'D' and mv.status<>'D')x
where rank=1 and scheme_commissioned_date is  null)xx


left join
-------------------------------------------set 2 for Balance Amount-----------------------------------


(select sh.location_id,sh.scheme_id,sum(vh.amount) expenditure
from prwss_main.t_finance_voucher_hdr vh--,prwss_main.t_finance_voucher_detail vd
,prwss_main.mst_scheme_header sh
where --vh.voc_id=vd.voc_id and
vh.scheme_id=sh.scheme_id
and   vh.status<>'D' --and vd.status<>'D'
and sh.status<>'D'
and   vh.voc_type='PAYMENT'
group  by sh.location_id,sh.scheme_id)xxx

on xx.scheme_id=xxx.scheme_id

left join
------------------------------------------------------------------------------------------------------
(select * from
(select scheme_id,
       sum(case when voc_type='PAYMENT' and payee_payer_type='OFFICE' then amount/100000 end) as rel_to_office
from prwss_main.t_finance_voucher_hdr
where status<>'D' and scheme_id <>'N/A'
group by scheme_id)p
where rel_to_office is not null)xxxx

on xx.scheme_id=xxxx.scheme_id

left join
------------------------------------------------------------------------------------------------------
(select scheme_id, sum(estimate_package_cost) estimate_cost
from prwss_main.t_proc_package_header ph
where ph.status <>'D'
group by scheme_id
)est
on xx.scheme_id = est.scheme_id

--------------------------------------target comm date shit-----------------------------------------
left  join
(select location_id,scheme_id,sum(estimate_package_cost) as total_estimate_packages_cost,max(anticipated_expenditure_cost) as anticipated_expenditure,target_commissioning_date
from (select * from
prwss_main.t_proc_package_header ph
left join prwss_main.t_proc_package_works pw on ph.package_id = pw.package_id
where ph.status <>'D' and pw.status<>'D'
) p,prwss_main.mst_divisional md
where md.divisional_id=p.location_id
group by scheme_id,location_id,target_commissioning_date
)pl on pl.scheme_id=xx.scheme_id

------------------------------------------------------------------------------------------------------

)main,prwss_main.mst_block mb,prwss_main.mst_constituency mc,prwss_main.mst_divisional di,prwss_main.mst_district d,prwss_main.mst_circle c,prwss_main.mst_zone z
where main.block_id=mb.block_id
and   main.constituency_id=mc.constituency_id
and   main.location_id=di.divisional_id
and   di.district_id=d.district_id
and   d.circle_id=c.circle_id
and   c.zone_id=z.zone_id
and   mb.status<>'D' and mc.status<>'D'
)final
where $P!{where}
order by zone_name,circle_name,district_name,divisional_name]]>
	</queryString>
