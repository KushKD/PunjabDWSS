	<queryString>
		<![CDATA[--A1 again
select main.*,(fc_mh*100/mh_2008) as fc_mh_per,(fc_oh*100/oh_2008) as fc_oh_per,(fc_total*100/total_2008) as fc_total_per,
	per_households,parti_habitation
from
(select mz.zone_id,zone_name,mc.circle_id,circle_name,md.district_id,district_name,per_rural_households_as_survey as per_households,
      sum(case when habitation_type='MH' then 1 else 0 end) as mh_2008,
      sum(case when habitation_type='OH' then 1 else 0 end) as oh_2008,
      sum(case when habitation_type in ('MH','OH') then 1 else 0 end) as total_2008,

 sum(case when (nc_pc_status1='FC' and habitation_type='MH') or (habitation_type='MH' and nc_pc_status1 not in ('NC','PC')
and scheme_commissioned_date>=to_date($P{from_date},'yyyy-mm-dd') and scheme_commissioned_date<=to_date($P{to_date},'yyyy-mm-dd'))
 then 1 else 0 end) as fc_mh,


sum(case when (nc_pc_status1='FC' and habitation_type='OH') or (habitation_type='OH' and nc_pc_status1 not in ('NC','PC')
		and scheme_commissioned_date>=to_date($P{from_date},'yyyy-mm-dd') and scheme_commissioned_date<=to_date($P{to_date},'yyyy-mm-dd') and scheme_source<>'SEWERAGE') then 1 else 0 end) as fc_oh,








 sum(case when (habitation_type in ('MH','OH') and nc_pc_status1='FC') or (habitation_type in ('MH','OH') and nc_pc_status1 in ('NC','PC')
		and scheme_commissioned_date>to_date($P{from_date},'yyyy-mm-dd') and scheme_commissioned_date<=to_date($P{to_date},'yyyy-mm-dd') and scheme_source<>'SEWERAGE') then 1 else 0 end) as fc_total,
      sum(case when sew_vill is not null and scheme_commissioned_date>to_date($P{from_date},'yyyy-mm-dd') and scheme_commissioned_date<=to_date($P{to_date},'yyyy-mm-dd')
		then 1 else 0 end) as parti_habitation


from(select village_id,village_name,divisional_office_id,habitation_type,nc_pc_status,spl_flags,scheme_commissioned_date,scheme_source,sew_vill,case when (nc_pc_status in ('PC','NC') and scheme_commissioned_date is not null and scheme_commissioned_date>=to_date($P{from_date},'yyyy-mm-dd') and scheme_commissioned_date<=to_date($P{to_date},'yyyy-mm-dd')) or spl_flags is not null then 'FC' else nc_pc_status end as nc_pc_status1
from (select * from
(select x.*,scheme_commissioned_date,scheme_source
from
(select mv.village_id,village_name,divisional_office_id,habitation_type,nc_pc_status,spl_flags
from prwss_main.mst_village mv,prwss_main.tbl_proposed_program_2008 ncpc
where mv.village_id=ncpc.village_id
and   mv.status<>'D') x
left join
(select * from
(select distinct location_id,sh.scheme_id,scheme_name,prog_id as program_id,sh.scheme_source,sh.scheme_upgraded,water_works_existing_new,admin_app_gop_letter_no,admin_app_gop_letter_date,village_id,
	scheme_commissioned_date,swap_nonswap,program_name,case when scheme_operated_by is not null and row_id=1 then scheme_operated_by else null end as scheme_operated_by,
	supply_service_level,case when  row_id=1  and (ro_plant=TRUE or ro_plant=FALSE) then ro_plant else null  end as ro_plant,row_id,prog_id,
	rank() over (partition by village_id order by scheme_commissioned_date desc,sh.scheme_id desc,admin_app_gop_letter_date desc,water_works_existing_new) as rank
from prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd,prwss_main.mst_program p,
(
select distinct scheme_id,village_id,scheme_commissioned_date,scheme_operated_by,ro_plant,scheme_upgraded,supply_service_level,row_id from
(select sv.*,rank() over (partition by village_id order by scheme_commissioned_date desc,sv.scheme_upgraded) as row_id
from prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd
where sh.scheme_id=sv.scheme_id and sv.scheme_id=sd.scheme_id and sh.status<>'D' and sv.status<>'D' and sd.status<>'D'
and   sh.scheme_source<>'SEWERAGE' and sh.scheme_source<>'IMPROVEMENT'
and   scheme_commissioned_date <=now()
order by village_id) con
where row_id in (1)
)sv
where sh.scheme_id=sd.scheme_id
and sd.scheme_id=sv.scheme_id
and sh.prog_id=p.program_id
and sh.status<>'D'
and sh.scheme_upgraded = 'NO'
and sd.status<>'D' )abc where rank=1) sch
on x.village_id=sch.village_id) xx2
left join
(select village_id as sew_vill
from prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh
where sh.scheme_id=sv.scheme_id and sh.status<>'D' and sv.status<>'D'
and   sh.scheme_source= 'SEWERAGE'
and   scheme_commissioned_date <=now()
order by village_id) sx
on xx2.village_id = sx.sew_vill)xxx)rrr,
prwss_main.pmm_table_1 tb1,prwss_main.mst_divisional mdi,prwss_main.mst_district md,prwss_main.mst_circle mc,prwss_main.mst_zone mz
where rrr.divisional_office_id=mdi.divisional_id
and   mdi.district_id=md.district_id
and   md.district_id=tb1.district_id
and   md.circle_id=mc.circle_id
and   mc.zone_id=mz.zone_id
group by mz.zone_id,zone_name,mc.circle_id,circle_name,md.district_id,district_name,per_rural_households_as_survey )main
where $P!{where}
order by district_name]]>
	</queryString>
