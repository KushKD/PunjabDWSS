	<queryString>
		<![CDATA[select district_name,  target_ws,comm_plan_time_cost, comm_out_plan_time_cost,(case when target_ws = 0 then 0 else (comm_plan_time_cost*100/target_ws) end) as ws_per,
	target_imp,achievement_in_time, achievement_outside_time, (case when target_imp =0 then 0 else (achievement_in_time*100/target_imp) end )as imp_per,
	target_sew,with_in_time_sew, out_side_time_sew, (case when target_sew =0 then 0 else (with_in_time_sew*100/target_sew) end ) as sew_per from

(select district_id,

	count(case when scheme_source not in ('SEWERAGE','IMPROVEMENT') then village_id else null end) as target_ws,
	count(case when prwss_main.datediff('month',loc_release_date,scheme_commissioned_date)<=12 and  scheme_source not in ('SEWERAGE','IMPROVEMENT') then village_id else null end) as comm_plan_time_cost,
	count(case when prwss_main.datediff('month',loc_release_date,scheme_commissioned_date)> 12 and  scheme_source not in ('SEWERAGE','IMPROVEMENT') then village_id else null end) as comm_out_plan_time_cost,
	count(case when scheme_source = 'IMPROVEMENT' then village_id else null end) as target_imp,
	count(case when prwss_main.datediff('month',loc_release_date,scheme_commissioned_date)<=4 and scheme_source='IMPROVEMENT'  then x3.village_id else null end) as achievement_in_time,
	count(case when prwss_main.datediff('month',loc_release_date,scheme_commissioned_date)>4  and scheme_source='IMPROVEMENT'  then x3.village_id else null end) as achievement_outside_time,
	count(case when scheme_source = 'SEWERAGE' then village_id else null end) as target_sew,
	count(case when prwss_main.datediff('month',loc_release_date,scheme_commissioned_date)<=18 and scheme_source='SEWERAGE'  then x3.village_id else null end) as with_in_time_sew,
	count(case when prwss_main.datediff('month',loc_release_date,scheme_commissioned_date)>18  and scheme_source='SEWERAGE'  then x3.village_id else null end) as out_side_time_sew

 from
(select x2.*, loc_release_date from
(select distinct x.district_id,x.location_id,scheme_id,scheme_name, x.village_id,habitation_type,scheme_commissioned_date,scheme_source from
(select distinct sh.scheme_id,sh.scheme_name, village_id,sh.location_id,sh.district_id, scheme_commissioned_date,scheme_source from
prwss_main.t_proc_sub_plan_scheme sps, prwss_main.mst_scheme_header sh, prwss_main.mst_scheme_village sv
where sps.scheme_id = sh.scheme_id
and sps.scheme_id = sv.scheme_id
and sh.prog_id = 'PROG15'
and  sh.status <> 'D' and sps.status <> 'D' and sv.status <> 'D'
) x
left join prwss_main.mst_village mv
on x.village_id = mv.village_id where habitation_type = 'MH' and mv. status <> 'D' ) x2

left join (select scheme_id,loc_release_date from
		(select distinct scheme_id,loc_release_date,rank() over (partition by scheme_id order by loc_release_date asc) r
		from prwss_main.t_finance_loc_header lh,prwss_main.t_finance_loc_detail ld
		where  lh.loc_id=ld.loc_id
		and    loc_for='Installment-1'
		and    lh.status<>'D' and ld.status<>'D' and loc_release_date is not null)sx where sx.r = 1)loc
on x2.scheme_id=loc.scheme_id) x3
group by district_id) x4


left join prwss_main.mst_district md
on x4.district_id = md.district_id
order by district_name]]>
	</queryString>
