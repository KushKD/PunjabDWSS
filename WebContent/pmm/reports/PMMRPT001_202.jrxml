	<queryString>
		<![CDATA[SELECT mv.village_id,
       mv.village_name,
       dp2.scheme_id,
       block_id,
       prwss_main.Get_location_name(block_id)            Block_Name,
       prwss_main.Get_location_Zone_by_VillId(mv.village_id) Zone_Name,
       prwss_main.Get_location_Circle_by_VillId(mv.village_id) Circle_Name,
       prwss_main.Get_location_District_by_VillId(mv.village_id) District_Name,
       prwss_main.get_location_divisional_by_villid(mv.village_id) Division_Name,
       disinfection_type,
       disinfection_instalation_time,
       disinfection_present_status,
       scheme_operated_by,
       dwss_operated_arrangement,
  sanctioned_load,
  pending__bill_31032017,
  pending__bill_30062017,
  avg_month_billof_wss,
  household_water_connection ,HHTotHoushlds ,
  water_connectio_sc_households,
  watered_connection ,
  metered_connection ,
  metered_supply_recovery ,
  flat_rate_charges_per_month ,
  arrear_of_recovery_with_consumers_as01042017
FROM   prwss_main.mst_village mv,
       prwss_main.draft_performa2 dp2,prwss_main.draft_performa4 dp4 ,prwss_main.draft_performa1 dp1 ,
       (SELECT mh.village_id                                    hhVillId,
               ( mh.sc_households + mh.gc_households )          HHTotHoushlds,
               mh.sc_households                                 HHSCHoushlds,
               Rank()
                 OVER (
                   partition BY village_id
                   ORDER BY as_on_date DESC, household_id DESC) AS row_id
        FROM   prwss_main.mst_village_households mh) HHTable
WHERE  mv.status <> 'D'
       AND dp2.village_id = mv.village_id
       AND dp4.village_id = mv.village_id
       AND dp4.village_id = dp2.village_id
       AND dp2.village_id = dp1.village_id
       AND dp1.village_id = HHTable.hhVillId

       AND $P!{where}

order by zone_name, circle_name, district_name, division_name, block_name]]>
	</queryString>
