	<queryString>
		<![CDATA[SELECT mv.village_id,
       mv.village_name,
       scheme_id,
       block_id,
       prwss_main.Get_location_name(block_id)            Block_Name,
       prwss_main.Get_location_Zone_by_VillId(mv.village_id) Zone_Name,
       prwss_main.Get_location_Circle_by_VillId(mv.village_id) Circle_Name,
       prwss_main.Get_location_District_by_VillId(mv.village_id) District_Name,
       prwss_main.get_location_divisional_by_villid(mv.village_id) Division_Name,
       CASE
         WHEN dp4.scheme_functional = 1 THEN 'Yes'
         ELSE 'No'
       END                                               AS scheme_functional,
       scheme_nonfunctional,
       scheme_nonfunctional_date,
       CASE
         WHEN dp4.ground_water_potable_status = 1 THEN 'Yes'
         ELSE 'No'
       END                                               AS ground_water_potable_status,
       ground_water_potable_no,
       preventive_measures_adopted,
       capacity_of_plant,
       date_of_installation,
       being_operated_by,
       volume_of_water_daily_basis ,
  disposal_of_reject_water ,
  penetration_in_percentage ,
  present_rate_of_user_charges ,
  seperate_sanctioned_load ,
  seperate_pending_eletric_bill31032017 ,
  average_monthly_bill_of_treatment_plant
FROM   prwss_main.mst_village mv,
       prwss_main.draft_performa4 dp4
WHERE  mv.status <> 'D'
       AND dp4.village_id = mv.village_id
	AND $P!{where}

order by zone_name, circle_name, district_name, division_name, block_name]]>
	</queryString>
