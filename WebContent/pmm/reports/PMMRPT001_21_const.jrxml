	<queryString>
		<![CDATA[select * from
(select zone_id,zone_name as zone,circle_id,circle_name as circle,district_id,district_name,constituency_id,constituency_name,program_id,program_name,swapnonswap as swap_nonswap,
count(case when scheme_id is not null and row_sch=1 then scheme_id else null end )as sch_cnt,
count(distinct case when swap_nonswap='SWAP'   and row_sch=1 then scheme_id else null end) swap,
count(distinct case when swap_nonswap='NONSWAP'  and row_sch=1 then scheme_id else null end) non_swap,
count(distinct case when water_works_existing_new='NEW' and row_sch=1 then scheme_id else null end) new_water_works,
count(distinct case when water_works_existing_new='EXISTING' and row_sch=1 then scheme_id else null end) existing_water_works,
count(distinct case when scheme_source ='CANAL' and row_schcnt=1 then scheme_id else null end) canal_sh,
count(distinct case when scheme_source='TUBEWELL'  and  row_schcnt=1  then scheme_id
	   when scheme_source='PERCULATIONWELL' and row_schcnt=1 then scheme_id
           when scheme_source='TUBEWELLWITHRO' and row_schcnt=1 then scheme_id
           when scheme_source='PONDS' and row_schcnt=1 then scheme_id
           when scheme_source='IMPROVEMENT' and row_schcnt=1 then scheme_id
           when scheme_source='DISTRIBUTION' and row_schcnt=1 then scheme_id
           when scheme_source='LIFTINGOFWATERFROMRSDLAKE' and row_schcnt=1 then scheme_id else null end) tubewell_sh,
count(distinct case when scheme_source='HANDPUMP' and row_schcnt=1  then scheme_id else null end) handpump_sh,
sum(case when habitation_type='MH' and (scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd') ) then 1 else 0 end) mh_habitation,
sum(case when habitation_type='OH' and (scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd') ) then 1 else 0 end) oh_habitation,
sum(case when habitation_type='MH' and ncpc_2008='NC' and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd') then 1 else 0 end) nc_habitation,
sum(case when habitation_type='MH' and ncpc_2008='PC' and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd') then 1 else 0 end) pc_habitation,
sum(case when habitation_type='MH' and ncpc_2008='FC' and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd') then 1 else 0 end) fc_upgraded,
sum(case when scheme_source='CANAL' and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd') then 1 else 0 end) canal,
sum(case when scheme_source='TUBEWELL' and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd')  then 1
	   when scheme_source='PERCULATIONWELL' and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd')  then 1
	   when scheme_source='PONDS'  and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd')  then 1
	   when scheme_source='IMPROVEMENT' and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd')  then 1
           when scheme_source='TUBEWELLWITHRO' and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd')  then 1
           when scheme_source='DISTRIBUTION'  and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd')  then 1
           when scheme_source='LIFTINGOFWATERFROMRSDLAKE'  and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd') then 1 else 0 end) tubewell,
sum(case when scheme_source='HANDPUMP' and scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd')  then 1 else 0 end ) handpump
from
(select z.zone_id,zone_name,c.circle_id,circle_name,d.district_id,district_name,b.constituency_id,constituency_name,program_id,
	prwss_main.get_program_name(program_id) as program_name,water_works_existing_new,
	scheme_id,scheme_source,swap_nonswap,habitation_type,row_sch,ncpc_2008,scheme_commissioned_date,swapnonswap,row_schcnt,present_status
from
(select divisional_office_id as location_id,constituency_id,
	case when scheme_id is null then 'TW' else scheme_id end as scheme_id,
	case when scheme_source is null then 'TUBEWELL' else scheme_source end as scheme_source,
	case when swap_nonswap is null then 'NONSWAP' else swap_nonswap end as swap_nonswap,
	case when program_id is null then 'PROG07' else program_id end as program_id,
	case when water_works_existing_new is null then 'NEW' else water_works_existing_new end as water_works_existing_new,
	habitation_type,row_sch,ncpc_2008,scheme_commissioned_date,swapnonswap,row_schcnt,present_status
from
(select x.divisional_office_id,x.constituency_id,xx.location_id,xx.scheme_id,xx.water_works_existing_new,ncpc_2008,xx.scheme_source,xx.program_id,xx.swap_nonswap,x.habitation_type,
	case when (x.nc_pc_status in ('PC','NC') and comm_date is not null and comm_date>='2008-04-01') or spl_flags is not null then 'FC' else x.nc_pc_status end as present_status
	,rank() over (partition by xx.scheme_id order by district_id) row_sch,
	rank() over (partition by xx.scheme_id order by district_id,divisional_office_id,constituency_id,block_id) row_schcnt,scheme_commissioned_date,swapnonswap
from
(select x.*,scheme_id,scheme_commissioned_date as comm_date,prog_id,
case when ncpc_2008 in ('NC','PC') and scheme_commissioned_date is not null then 'FC' else ncpc_2008 end as nc_pc_status
from
(select mv.*,nc_pc_status as ncpc_2008
from prwss_main.mst_village mv,prwss_main.tbl_proposed_program_2008 ncpc
where mv.village_id=ncpc.village_id
and   mv.status<>'D')x
left join
(select * from
(select mv.village_id,sh.scheme_id,scheme_commissioned_date,prog_id,
rank() over (partition by mv.village_id order by scheme_commissioned_date desc, sh.scheme_id desc) row_id
from prwss_main.mst_village mv,prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd
where mv.village_id=sv.village_id
--and   sh.scheme_source not in ( 'SEWERAGE','IMPROVEMENT')
and   sh.scheme_id=sv.scheme_id and sv.scheme_id=sd.scheme_id and sh.status<>'D' and sd.status<>'D'
and   mv.status<>'D' and sv.status<>'D'
and   scheme_commissioned_date>=to_date('2008-04-01','yyyy-mm-dd') and scheme_commissioned_date < now())y where row_id=1)xx
on x.village_id=xx.village_id
--where habitation_type='MH'
) x

left join
(select * from
(select distinct location_id,sh.scheme_id,scheme_name,prog_id as program_id,sh.scheme_source,water_works_existing_new,admin_app_gop_letter_no,admin_app_gop_letter_date,village_id,
	scheme_commissioned_date,swap_nonswap,program_name,case when scheme_operated_by is not null and row_id=1 then scheme_operated_by else null end as scheme_operated_by,
	supply_service_level,case when  row_id=1  and (ro_plant=TRUE or ro_plant=FALSE) then ro_plant else null  end as ro_plant,row_id,
	rank() over (partition by village_id order by scheme_commissioned_date desc,sh.scheme_id desc,admin_app_gop_letter_date desc,water_works_existing_new) as rank,
	case when p.swap_nonswap='SWAP' and program_id='PROG15' then  'SWAP-IDA'
	     when p.swap_nonswap='SWAP' and program_id<>'PROG15' then 'SWAP-NON IDA'
	     else p.swap_nonswap end swapnonswap
from prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd,prwss_main.mst_program p,
(
select distinct scheme_id,village_id,scheme_commissioned_date,scheme_operated_by,ro_plant,supply_service_level,row_id from
(select sv.*,rank() over (partition by village_id order by scheme_commissioned_date desc,sh.scheme_id desc,admin_app_gop_letter_date desc) as row_id
from prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd
where sh.scheme_id=sv.scheme_id and sv.scheme_id=sd.scheme_id and sh.status<>'D' and sv.status<>'D' and sd.status<>'D'
and   sh.scheme_source not in ('SEWERAGE')
and   scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd')
order by village_id) con
where row_id in (1)
)sv
where sh.scheme_id=sd.scheme_id
and sd.scheme_id=sv.scheme_id
and sh.prog_id=p.program_id
and sh.status<>'D'
and sd.status<>'D' )abc where rank=1)xx
on x.village_id=xx.village_id)main
where present_status<>'NC')xyz,prwss_main.mst_constituency b,prwss_main.mst_district d,prwss_main.mst_circle c,prwss_main.mst_zone z
where xyz.constituency_id=b.constituency_id
and   b.district_id=d.district_id
and   d.circle_id=c.circle_id
and   c.zone_id=z.zone_id
order by scheme_id) sd
group by zone_id,zone_name,circle_id,circle_name,district_id,district_name,constituency_id,constituency_name,program_id,
program_name,swapnonswap
order by zone_name,circle_name,district_name,constituency_name,program_name,swapnonswap) main
where $P!{where}
order by zone,circle,district_name,constituency_name,program_name]]>
	</queryString>
