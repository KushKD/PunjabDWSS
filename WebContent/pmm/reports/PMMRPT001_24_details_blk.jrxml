	<queryString>
		<![CDATA[select  zone_id,zone_name,circle_id,circle_name,district_id,district_name,block_id,block_name,divisional_id,divisional_name,
	replace(scheme_name,'{}','') scheme_name,village_id,village_name,ncpc_2008,nc_pc_status,present_status,mnth_yr,
	replace(cover_under,'{','') cover_under,
	split_part(replace(scheme_operated_by,'}',''), '-',1) scheme_operated_by,

	case when scheme_operated_by like '%Self%' then 'Self'
	when scheme_operated_by like '%Outsourced%' then 'Outsourced'
	else '' end scheme_operated_through,

	replace(supply_service_level,'}','')supply_service_level,
	replace(ro_plant,'{f}','No')ro_plant,name_of_prog,gen_population,sc_population,st_population,population,
	water_connection,no_of_standpost,sc_households,gc_households,households,replace(scheme_commissioned_date,'}','') scheme_commissioned_date,
	replace(prev_comm,'{','') prev_comm, scheme_source,habitation_type,location_water_works,

	case when location_water_works is not null and location_water_works='Y' then village_name else '' end lww_village_name,
	case when location_water_works is not null and location_water_works='Y' then habitation_type else '' end lww_habitation_type,
	case when location_water_works is not null and location_water_works='Y' then village_id else '' end lww_village_id


from
(select  zone_id,zone_name,circle_id,circle_name,district_id,district_name,block_id,block_name,divisional_id,divisional_name,
	replace(scheme_name,'{}','') scheme_name,village_id,village_name,ncpc_2008,nc_pc_status,present_status,mnth_yr,
	replace(cover_under,'}','') cover_under,replace(scheme_operated_by,'{','')scheme_operated_by,replace(supply_service_level,'{','')supply_service_level,
	replace(ro_plant,'{t}','Yes')ro_plant,name_of_prog,gen_population,sc_population,st_population,population,
	water_connection,no_of_standpost,sc_households,gc_households,households,replace(scheme_commissioned_date,'{','') scheme_commissioned_date,
	replace(prev_comm,'}','') prev_comm, scheme_source,habitation_type,location_water_works
from
(select zone_id,zone_name,circle_id,circle_name,district_id,district_name,block_id,block_name,divisional_id,divisional_name,
	replace(scheme_name,'{}','') scheme_name,village_id,village_name,ncpc_2008,nc_pc_status,present_status,mnth_yr,
	replace(cover_under,',','') cover_under,replace(scheme_operated_by,',','') scheme_operated_by,replace(supply_service_level,',','') supply_service_level,
	replace(ro_plant,',','')ro_plant,
	replace(name_of_prog,'"','')name_of_prog,gen_population,sc_population,st_population,population,
	water_connection,no_of_standpost,sc_households,gc_households,households,replace(scheme_commissioned_date,',','')scheme_commissioned_date,
	replace(prev_comm,',','') prev_comm, scheme_source,habitation_type,location_water_works
from
(select  zone_id,zone_name,circle_id,circle_name,district_id,district_name,block_id,block_name,divisional_id,divisional_name,
	replace(cast(array_agg(scheme_name)as text),'NULL','') as scheme_name,village_id,
	village_name,ncpc_2008,	nc_pc_status,present_status,comm_date,mnth_yr,replace(cast(array_agg(cover_under)as text),'NULL','') cover_under,
	replace(cast(array_agg(scheme_operated_by)as text),'NULL','') scheme_operated_by,
	replace(cast(array_agg(supply_service_level)as text),'NULL','') supply_service_level,
	replace(cast(array_agg(ro_plant)as text),'NULL','') ro_plant,
	replace(cast(array_agg(name_of_prog)as text),'NULL','') name_of_prog,gen_population,sc_population,st_population,population,water_connection,no_of_standpost,
	sc_households,gc_households,households,
	replace(cast(array_agg(scheme_commissioned_date) as text),'NULL','') scheme_commissioned_date,
	replace(cast(array_agg(prev_comm) as text),'NULL','') prev_comm,scheme_source,habitation_type,location_water_works

	--case when scheme_commissioned_date in ('Prior to 01-04-2008','Yet to be taken up') then null else scheme_name end as sch_name
from (
select distinct zone_id,zone_name,circle_id,circle_name,district_id,district_name,block_id,block_name,divisional_id,divisional_name,
	case when prev_comm is not null then null else scheme_name end as scheme_name,
	village_id,village_name,ncpc_2008,nc_pc_status,present_status,comm_date,mnth_yr,
	case when prev_comm is not null then null else cover_under end as cover_under,scheme_operated_by,supply_service_level,ro_plant,
	case when prev_comm is not null then null else name_of_prog end as name_of_prog,gen_population,sc_population,st_population,
	population,water_connection,no_of_standpost,sc_households,gc_households,households,scheme_commissioned_date,prev_comm,scheme_source,habitation_type,location_water_works
from
(select z.zone_id,z.zone_name,c.circle_id,c.circle_name,d.district_id,d.district_name,b.block_id,b.block_name,dv.divisional_id,dv.divisional_name,
scheme_name||' ('||scheme_id||')' as scheme_name, scheme_source,
x.village_id,x.village_name||' ('||x.village_id||')' as village_name,
ncpc_2008,
x.nc_pc_status,x.habitation_type,location_water_works,
case when x.nc_pc_status in ('PC','NC') and scheme_commissioned_date is not null and scheme_commissioned_date>='2008-04-01' then 'FC' else x.nc_pc_status end as present_status,
comm_date,

EXTRACT(month from to_date('$P!{to_date}','yyyy-mm-dd'))||'/'||EXTRACT(year from to_date('$P!{to_date}','yyyy-mm-dd')) mnth_yr,

case when xcom.prog_id is not null then xcom.swap_nonswap
when xcom.prog_id is null and x.nc_pc_status='FC' then 'NA'
when xcom.prog_id is null and (x.nc_pc_status='PC' or x.nc_pc_status='NC') then pro.proposed_swap_nonswap end cover_under,
scheme_operated_by,supply_service_level,
ro_plant,

case when xcom.prog_id is not null then xcom.program_name
when xcom.prog_id is null and x.ncpc_2008='FC' then 'NA'
when xcom.prog_id is null and (x.ncpc_2008='PC' or x.nc_pc_status='NC') and pro.proposed_program_id='PROG15' then 'PRWSS Project'
when xcom.prog_id is null and (x.ncpc_2008='PC' or x.nc_pc_status='NC') and pro.proposed_program_id<>'PROG15' then pro.proposed_program_name
end name_of_prog,

gen_population,sc_population,st_population,
total_population as population,no_of_water_connection as water_connection,no_of_standpost,sc_households,gc_households,households,

admin_app_gop_letter_no,
case
when scheme_commissioned_date is null and (x.ncpc_2008='PC' or x.ncpc_2008='NC') and admin_app_gop_letter_no is not null and row_id=1 then 'Under Progress'
when scheme_commissioned_date is null and admin_app_gop_letter_no is null and (x.ncpc_2008='PC' or x.ncpc_2008='FC') and row_id=1 then 'Prior to 01-04-2008'
when scheme_commissioned_date is null and admin_app_gop_letter_no is null and (x.ncpc_2008 in ('NC','PC')) and (row_id=1 or row_id is null) and spl_flags is null then 'Yet to be taken up'
when scheme_commissioned_date is null and admin_app_gop_letter_no is null and (x.ncpc_2008 in ('NC','PC')) and (row_id=1 or row_id is null) and spl_flags is not null then spl_flags

when scheme_commissioned_date is  null and x.ncpc_2008='FC' and admin_app_gop_letter_no is not null --and scheme_commissioned_date>'2008-04-01'
and row_id=1 then  '(Being upgraded)'
--when scheme_commissioned_date is not null and admin_app_gop_letter_no is not null and scheme_commissioned_date<'2008-04-01' and (x.nc_pc_status='PC' or x.nc_pc_status='NC') then 'Under Progress'
when scheme_commissioned_date is not null and (x.ncpc_2008='PC' or x.ncpc_2008='NC' or x.ncpc_2008='FC') and row_id=1 then to_char(scheme_commissioned_date, 'MON/yyyy')
--when scheme_commissioned_date is not null and admin_app_gop_letter_no is not null and (x.nc_pc_status='PC' or x.nc_pc_status='NC') and scheme_commissioned_date<'2008-04-01' then 'Yet to be taken up'

--when scheme_commissioned_date is not null and x.nc_pc_status='FC' and scheme_commissioned_date>'2008-04-01' then to_char(scheme_commissioned_date, 'MON/yyyy') || '(Upgraded)'
--when scheme_commissioned_date is not null and (x.nc_pc_status='FC') then to_char(scheme_commissioned_date, 'MON/yyyy')

end as scheme_commissioned_date,
case when scheme_commissioned_date is not null and row_id<>1 and x.ncpc_2008 in ('NC','PC','FC') then to_char(scheme_commissioned_date, 'MON/yyyy') else null end as prev_comm

from (
select x.*,scheme_commissioned_date as comm_date,case when (ncpc_2008 in ('NC','PC') and scheme_commissioned_date is not null) or spl_flags is not null then 'FC' else ncpc_2008 end as nc_pc_status
from
(select mv.*,nc_pc_status as ncpc_2008
from prwss_main.mst_village mv,prwss_main.tbl_proposed_program_2008 ncpc
where mv.village_id=ncpc.village_id
and   mv.status<>'D')x
------------------

left join
(select * from
(select mv.village_id,scheme_commissioned_date,mv.habitation_type,rank() over (partition by mv.village_id order by scheme_commissioned_date desc, scheme_id desc) row_id
from prwss_main.mst_village mv,prwss_main.mst_scheme_village sv
where mv.village_id=sv.village_id
and   mv.status<>'D' and sv.status<>'D'
and   scheme_commissioned_date>'2008-04-01' and scheme_commissioned_date < '$P!{to_date}')y where row_id=1)xx
on x.village_id=xx.village_id
----where habitation_type='MH'
) x

---------------------------------population---------------------------------------------
left outer join
(
select population_id,village_id,gen_population,sc_population,st_population,(gen_population+sc_population+st_population)total_population,population_year,as_on_date
from
(select *,rank() over (partition by village_id order by as_on_date desc,population_id desc) as row_id
from prwss_main.mst_village_population mp
where as_on_date <=to_date('$P!{to_date}','yyyy-MM-dd')
and status<>'D'
order by village_id) pop
where row_id=1
)xp on x.village_id=xp.village_id
---------------------------------pvt connection ---------------------------------------------
left outer join
(
select connection_id,village_id,no_of_water_connection,no_of_standpost,as_on_date from
(select *,rank() over (partition by village_id order by as_on_date desc,connection_id desc) as row_id
from prwss_main.mst_village_connection
where as_on_date <=to_date('$P!{to_date}','yyyy-MM-dd')
and status<>'D'
order by village_id) con
where row_id=1
)xpc on x.village_id=xpc.village_id


---------------------------------household ---------------------------------------------
left outer join
(
select household_id,village_id,sc_households,gc_households,(sc_households+gc_households) as households,as_on_date from
(select *,rank() over (partition by village_id order by as_on_date desc,household_id desc) as row_id
from prwss_main.mst_village_households
where as_on_date <=to_date('$P!{to_date}','yyyy-MM-dd')
and status<>'D'
order by village_id) con
where row_id=1
)xh on x.village_id=xh.village_id
---------------------------------commissioning ---------------------------------------------
left outer join
(
select  scheme_id,scheme_name,prog_id,admin_app_gop_letter_no,admin_app_gop_letter_date,village_id,scheme_commissioned_date,swap_nonswap,program_name,scheme_source,
	case when scheme_operated_by is not null and row_id=1 then scheme_operated_by else null end as scheme_operated_by,
	case when supply_service_level is not null and row_id=1 then supply_service_level else null end as supply_service_level,
	case when  row_id=1  and (ro_plant=TRUE or ro_plant=FALSE) then ro_plant else null  end as ro_plant,row_id,location_water_works
from
(
select scheme_id,scheme_name,prog_id,admin_app_gop_letter_no,admin_app_gop_letter_date,village_id,scheme_commissioned_date,swap_nonswap,program_name,scheme_operated_by,supply_service_level,scheme_source,ro_plant,row_id,location_water_works
from
(select sh.scheme_name,sh.prog_id,admin_app_gop_letter_no,admin_app_gop_letter_date,swap_nonswap,program_name,
	sv.*,rank() over (partition by village_id order by scheme_commissioned_date desc,sh.scheme_id desc,admin_app_gop_letter_date desc,sh.scheme_upgraded) as row_id, sh.scheme_source
from prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd,prwss_main.mst_program p
where sh.scheme_id=sv.scheme_id
and sv.scheme_id=sd.scheme_id
and  sh.prog_id=p.program_id
and  sh.scheme_upgraded=sv.scheme_upgraded
and  sv.scheme_upgraded=sd.scheme_upgraded
and sh.status<>'D' and sv.status<>'D' and sd.status<>'D'
and sh.scheme_source<>'SEWERAGE'
and scheme_commissioned_date <=to_date('$P!{to_date}','yyyy-MM-dd')
and admin_app_gop_letter_no is not null
order by village_id) con
where row_id in (1,2)
)sv
)xcom
 on x.village_id=xcom.village_id
, prwss_main.mst_zone z,prwss_main.mst_circle c, prwss_main.mst_district d, prwss_main.mst_divisional dv,prwss_main.mst_block b,prwss_main.tbl_proposed_program_2008 pro,prwss_main.mst_constituency cnt
where z.zone_id=c.zone_id
and c.circle_id=d.circle_id
and d.district_id=b.district_id
and x.divisional_office_id=dv.divisional_id
and x.block_id=b.block_id
and x.village_id=pro.village_id
and x.constituency_id=cnt.constituency_id )xyz
) main
where $P!{where}
group by zone_id,zone_name,circle_id,circle_name,district_id,district_name,block_id,block_name,divisional_id,divisional_name,village_id,village_name,ncpc_2008,
	nc_pc_status,present_status,comm_date,mnth_yr,gen_population,sc_population,st_population,population,water_connection,no_of_standpost,sc_households,gc_households,
	households,scheme_source,habitation_type,location_water_works
	)yy)xxxx)xyz
order by zone_name,circle_name,district_name,block_name asc,village_name asc]]>
