	<queryString>
		<![CDATA[select location_id,sch_name as sch_admin_ap,village,null sch_tech,null village_tech,
	null tender_inv,null tender_allotd,null sch_comm,null vill_comm,null sch_compl,null conn,swapnonswap
from
(select sh.location_id,(scheme_name||' ('||sh.scheme_id||')') as sch_name,
	village_name||' ('||mv.village_id||')' as village,
	case when prog_id='PROG15' then 'SWAP-IDA'
	     when prog_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'
	     when prog_id not in ('PROG15','PROG06','PROG11','PROG20') then 'NONSWAP' end as swapnonswap
from prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_details sd,prwss_main.mst_village mv
where sh.scheme_id=sv.scheme_id
and   sv.scheme_id=sd.scheme_id
and   sv.village_id=mv.village_id --and sh.prog_id='PROG15'
and   sh.status<>'D' and   sv.status<>'D' and   sd.status<>'D' and   mv.status<>'D'
and   admin_app_gop_letter_date  between to_date('2011-04-01','yyyy-mm-dd') and to_date('2011-06-01','yyyy-mm-dd'))x

union all

select location_id,null sch_admin_ap,null village,
	sch_name as sch_tech,village_t as village_tech,
	null tender_inv,null tender_allotd,null sch_comm,null vill_comm,null sch_compl,null conn,swapnonswap
from
(select sh.location_id,(scheme_name||' ('||sh.scheme_id||')') as sch_name,village_name||' ('||mv.village_id||')' as village_t,
	case when prog_id='PROG15' then 'SWAP-IDA'
	     when prog_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'
	     when prog_id not in ('PROG15','PROG06','PROG11','PROG20') then 'NONSWAP' end as swapnonswap
from prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_details sd,prwss_main.mst_village mv
where sh.scheme_id=sv.scheme_id
and   sv.scheme_id=sd.scheme_id
and   sv.village_id=mv.village_id --and sh.prog_id='PROG15'
and   sh.status<>'D' and   sv.status<>'D' and   sd.status<>'D' and   mv.status<>'D'
and   (tech_app_ce_letter_date  between to_date('2011-04-01','yyyy-mm-dd') and to_date('2011-06-01','yyyy-mm-dd')
	or tech_app_se_letter_date  between to_date('2011-04-01','yyyy-mm-dd') and to_date('2011-06-01','yyyy-mm-dd')
	or tech_app_ee_date  between to_date('2011-04-01','yyyy-mm-dd') and to_date('2011-06-01','yyyy-mm-dd'))
)xx

union all

select location_id,null sch_admin_ap,null as village,
	null sch_tech,null as village_tech,
	null tender_inv,sch_name as tender_allotd,null sch_comm,null vill_comm,null sch_compl,null conn,swapnonswap
from
(select sh.location_id,(sh.scheme_name||' ('||sh.scheme_id||')') sch_name,
	case when prog_id='PROG15' then 'SWAP-IDA'
	     when prog_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'
	     when prog_id not in ('PROG15','PROG06','PROG11','PROG20') then 'NONSWAP' end as swapnonswap
from prwss_main.mst_scheme_header sh,prwss_main.t_proc_package_header ph,prwss_main.t_tender_hdr th,prwss_main.t_tender_award ta
where sh.scheme_id=ph.scheme_id
and   ph.scheme_id=th.scheme_code
and   th.tender_id=ta.tender_id --and sh.prog_id='PROG15'
and   sh.status<>'D' and ph.status<>'D' and th.status<>'D' and ta.status<>'D'
and   signing_of_contract between to_date('2011-04-01','yyyy-mm-dd') and to_date('2011-06-01','yyyy-mm-dd'))xxx

union all

select location_id,null sch_admin_ap,null as village,
	null sch_tech,null as village_tech,
	null tender_inv,null tender_allotd,sch_name as sch_comm,vill_id as vill_comm,null sch_compl,null conn,swapnonswap
from
(select sh.location_id,(scheme_name||' ('||sh.scheme_id||')') as sch_name,(village_name||' ('||mv.village_id||' )') as vill_id,
	case when habitation_type='MH' then village_name||' ('||mv.village_id||' )' else null end as vill_mh,
	case when habitation_type='OH' then village_name||' ('||mv.village_id||' )' else null end as vill_oh,
	case when prog_id='PROG15' then 'SWAP-IDA'
	     when prog_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'
	     when prog_id not in ('PROG15','PROG06','PROG11','PROG20') then 'NONSWAP' end as swapnonswap
from prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_details sd,prwss_main.mst_village mv
where sh.scheme_id=sv.scheme_id
and   sv.scheme_id=sd.scheme_id
and   sv.village_id=mv.village_id --and sh.prog_id='PROG15'
and   sh.status<>'D' and   sv.status<>'D' and   sd.status<>'D' and   mv.status<>'D'
and   scheme_commissioned_date  between to_date('2011-04-01','yyyy-mm-dd') and to_date('2011-06-01','yyyy-mm-dd'))xxxx

union all

select location_id,null sch_admin_ap,null as village,
	null sch_tech,null as village_tech,
	null tender_inv,null tender_allotd,sch_name as sch_comm,null vill_comm,sch_name as sch_compl,null conn,swapnonswap
from
(select sh.location_id,(scheme_name||' ('||sh.scheme_id||')') as sch_name,mv.village_id,
	case when habitation_type='MH' then mv.village_id else null end as vill_mh,
	case when habitation_type='OH' then mv.village_id else null end as vill_oh,
	case when prog_id='PROG15' then 'SWAP-IDA'
	     when prog_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'
	     when prog_id not in ('PROG15','PROG06','PROG11','PROG20') then 'NONSWAP' end as swapnonswap
from prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_details sd,prwss_main.mst_village mv
where sh.scheme_id=sv.scheme_id
and   sv.scheme_id=sd.scheme_id
and   sv.village_id=mv.village_id --and sh.prog_id='PROG15'
and   sh.status<>'D' and   sv.status<>'D' and   sd.status<>'D' and   mv.status<>'D'
and   sd.scheme_completed_date  between to_date('2011-04-01','yyyy-mm-dd') and to_date('2011-06-01','yyyy-mm-dd'))xxxx

union all





---------------------------------connection after commissioning up To previous----------------------------------------

select location_id,null sch_admin_ap,null as village,
	null sch_tech,null as village_tech,
	null tender_inv,null tender_allotd,null as sch_comm,null vill_comm,null as sch_compl,no_of_water_connection as  conn_up_to,null conn_during,swapnonswap
 from

(select mp.*,rank() over (partition by mp.village_id order by as_on_date desc,connection_id desc) as row_id,
	case when prog_id='PROG15' then 'SWAP-IDA'
	     when prog_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'
	     when prog_id not in ('PROG15','PROG06','PROG11','PROG20') then 'NONSWAP' end as swapnonswap

from (select b.divisional_office_id as location_id,village_name, a.* from prwss_main.mst_village_connection a,prwss_main.mst_village b
where a.village_id=b.village_id and a.status<>'D' and b.status<>'D') mp

left outer join

(select sh.scheme_id,village_id,scheme_commissioned_date,prog_id
from prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh
where sh.scheme_id=sv.scheme_id and sh.status<>'D' and sv.status<>'D') msv on mp.village_id=msv.village_id

where as_on_date < date_trunc('month', to_date('2012-07-01','yyyy-MM-dd'))
and msv.scheme_commissioned_date <= as_on_date
and mp.status<>'D'
order by mp.village_id) main

where row_id=1

union all
---------------------------------connection after commissioning during mnth---------------------------------------------

select location_id,null sch_admin_ap,null as village,
	null sch_tech,null as village_tech,
	null tender_inv,null tender_allotd,null as sch_comm,null vill_comm,null as sch_compl,null as  conn_up_to,no_of_water_connection as conn_during,swapnonswap
 from

(select mp.*,rank() over (partition by mp.village_id order by as_on_date desc,connection_id desc) as row_id,
	case when prog_id='PROG15' then 'SWAP-IDA'
	     when prog_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'
	     when prog_id not in ('PROG15','PROG06','PROG11','PROG20') then 'NONSWAP' end as swapnonswap

from (select b.divisional_office_id as location_id,village_name, a.* from prwss_main.mst_village_connection a,prwss_main.mst_village b
where a.village_id=b.village_id and a.status<>'D' and b.status<>'D') mp

left outer join

(select sh.scheme_id,village_id,scheme_commissioned_date,prog_id
from prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh
where sh.scheme_id=sv.scheme_id and sh.status<>'D' and sv.status<>'D') msv on mp.village_id=msv.village_id

where as_on_date <= to_date('2012-07-31','yyyy-MM-dd') and
as_on_date>=date_trunc('month', to_date('2012-07-31','yyyy-MM-dd'))
and msv.scheme_commissioned_date <= as_on_date
and mp.status<>'D'
order by mp.village_id) main

where row_id=1]]>
