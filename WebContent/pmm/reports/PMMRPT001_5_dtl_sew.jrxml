	<queryString>
		<![CDATA[select
zone_id,zone_name,circle_id,circle_name,district_id,district_name,village_id,village_name,total_population,total_households,no_of_water_connection_pre,scheme_commissioned_date,scheme_name,location_id,no_of_water_connection_after_up_to_pre,no_of_water_connection_after_during,
(no_of_water_connection_after_up_to_pre+no_of_water_connection_after_during) total_connection,connection_per_after,divisional_name,village,pre_mnth_conn,during_mnth_conn,
(pre_mnth_conn+during_mnth_conn) total
, EXTRACT(month from to_date($P{to_date},'yyyy-mm-dd'))||'/'||EXTRACT(year from to_date($P{to_date},'yyyy-mm-dd')) mnth_yr
from(
select distinct main.*,divisional_name from
(select z.zone_id,z.zone_name,c.circle_id,c.circle_name,d.district_id,d.district_name,--divisional_name,
---case when svd.program_id='PROG15' then 'WB/IDA' else 'Other than IDA' end as program_id,
svd.village_id,mv.village_name,total_population,total_households,---sv_scheme_commisioned_date,
no_of_water_connection_pre,scheme_commissioned_date,scheme_name||' ('||scheme_id||')' as scheme_name,location_id
,case when no_of_water_connection_after_up_to_pre>0 then no_of_water_connection_after_up_to_pre-no_of_water_connection_pre else 0 end no_of_water_connection_after_up_to_pre,
case when no_of_water_connection_after_during>0 then no_of_water_connection_after_during-
(case when no_of_water_connection_after_up_to_pre>0 then no_of_water_connection_after_up_to_pre else no_of_water_connection_pre end) else 0 end no_of_water_connection_after_during,
(no_of_water_connection_pre+
case when no_of_water_connection_after_up_to_pre>0 then no_of_water_connection_after_up_to_pre-no_of_water_connection_pre else 0 end+
case when no_of_water_connection_after_during>0 then no_of_water_connection_after_during-
(case when no_of_water_connection_after_up_to_pre>0 then no_of_water_connection_after_up_to_pre else no_of_water_connection_pre end) else 0 end
) as total_connection,

case when total_households<>0 then
((no_of_water_connection_pre+
case when no_of_water_connection_after_up_to_pre>0 then no_of_water_connection_after_up_to_pre-no_of_water_connection_pre else 0 end+
case when no_of_water_connection_after_during>0 then no_of_water_connection_after_during-
(case when no_of_water_connection_after_up_to_pre>0 then no_of_water_connection_after_up_to_pre else no_of_water_connection_pre end) else 0 end
)*100/total_households)

end connection_per_after
--------------------------------

from (select scheme_id,scheme_name,xx.village_id,scheme_commissioned_date, location_id,district_id from
(select distinct sh.scheme_id,sh.scheme_name,sv.village_id,sv.scheme_commissioned_date, sh.location_id,sh.district_id
from
prwss_main.t_proc_sub_plan_scheme sps, prwss_main.mst_scheme_header sh, prwss_main.mst_scheme_village sv
where sps.scheme_id = sh.scheme_id
and sh.scheme_id = sv.scheme_id
and scheme_commissioned_date is not null
and sv.status <> 'D' and sh.status <> 'D' and sps.status <> 'D' and (sps.scheme_id like '%SEW%')
and sv.scheme_commissioned_date <=to_date($P{to_date},'yyyy-MM-dd')
)
xx
left join
(select distinct village_id from
(select village_id, (sew_connection) as con,tariff_rate,as_on_date, rank() over (partition by village_id order by as_on_date desc nulls last,sewerage_tariff_id) wc_rank
 from prwss_main.mst_village_sewerage_tariff where status <> 'D'
 and as_on_date <= to_date($P{to_date},'yyyy-MM-dd')) wc  where
 wc_rank = 1 ) sx
on xx.village_id = sx.village_id
) svd

left outer join

(

select village_id,sum(total_population) total_population,sum(no_of_water_connection_pre) no_of_water_connection_pre,

sum(no_of_water_connection_after_during) no_of_water_connection_after_during
,sum(no_of_water_connection_after_up_to_pre) as no_of_water_connection_after_up_to_pre,sum(total_households) total_households---,sum(no_of_standpost_pre)no_of_standpost_pre,
----sum(no_of_standpost_after_during)no_of_standpost_after_during,sum(no_of_standpost_after_up_to_pre) no_of_standpost_after_up_to_pre

from

(---------------------------------population---------------------------------------------

select village_id,(gen_population+sc_population+st_population) as total_population,0 as no_of_water_connection_pre,0 as no_of_water_connection_after_during,0 as no_of_water_connection_after_up_to_pre,
0 as total_households
----0 no_of_standpost_pre,0 no_of_standpost_after_during,0 as no_of_standpost_after_up_to_pre
from
(select *,rank() over (partition by village_id order by as_on_date desc,population_id desc) as row_id

from prwss_main.mst_village_population mp

where as_on_date <=to_date($P{to_date},'yyyy-MM-dd')
and status<>'D'
order by village_id) main

where row_id=1



union all
---------------------------------connection after commissioning up To previous----------------------------------------

select distinct main.village_id,0 as total_population,0 as no_of_water_connection_pre,0 as no_of_water_connection_after_during,
sew_connection as no_of_water_connection_after_up_to_pre,0 as total_households
----0 no_of_standpost_pre,0 as no_of_standpost_after_during,no_of_standpost no_of_standpost_after_up_to_pre
 from

(select mp.*,rank() over (partition by mp.village_id order by as_on_date desc,sewerage_tariff_id desc) as row_id

from prwss_main.mst_village_sewerage_tariff mp left outer join prwss_main.mst_scheme_village msv on mp.village_id=msv.village_id

where as_on_date < date_trunc('month', to_date($P{to_date},'yyyy-MM-dd'))

and msv.scheme_commissioned_date <= as_on_date
and mp.status<>'D'
and msv.status<>'D'
order by mp.village_id) main

where row_id=1

union all
---------------------------------connection after commissioning during mnth---------------------------------------------

select distinct main.village_id,0 as total_population,0 as no_of_water_connection_pre,sew_connection as no_of_water_connection_after_during,
0 as no_of_water_connection_after_up_to_pre,0 as total_households
----0 no_of_standpost_pre,no_of_standpost no_of_standpost_after_during,0 as no_of_standpost_after_up_to_pre
 from

(select mp.*,rank() over (partition by mp.village_id order by as_on_date desc,sewerage_tariff_id desc) as row_id
from prwss_main.mst_village_sewerage_tariff mp left outer join prwss_main.mst_scheme_village msv on mp.village_id=msv.village_id
where as_on_date <= to_date($P{to_date},'yyyy-MM-dd') and
as_on_date>=date_trunc('month', to_date($P{to_date},'yyyy-MM-dd'))
and msv.scheme_commissioned_date <= as_on_date
and mp.status<>'D'
and msv.status<>'D'
order by mp.village_id) main
where row_id=1



union all



---------------------------------connection before  commissioning ---------------------------------------------

select distinct  main.village_id,0 as total_population,sew_connection as no_of_water_connection_pre,0 as no_of_water_connection_after_during,
0 as no_of_water_connection_after_up_to_pre,0 as total_households
-----no_of_standpost as no_of_standpost_pre,0 no_of_standpost_after,0 as no_of_standpost_after_up_to_pre
from
(select mp.*,rank() over (partition by mp.village_id order by as_on_date desc,sewerage_tariff_id desc) as row_id

from prwss_main.mst_village_sewerage_tariff mp left outer join prwss_main.mst_scheme_village msv on mp.village_id=msv.village_id

where  msv.scheme_commissioned_date>=as_on_date
and as_on_date <= to_date($P{to_date},'yyyy-MM-dd')
and mp.status<>'D'
and msv.status<>'D'
order by mp.village_id) main

where row_id=1

--8375

union all

---------------------------------household---------------------------------------------

select village_id,0 as total_population,0 as no_of_water_connection_pre,0 as no_of_water_connection_after_during,0 as no_of_water_connection_after_up_to_pre,
(sc_households+gc_households)total_households
-----0 no_of_standpost_pre,0 no_of_standpost_after,0 as no_of_standpost_after_up_to_pre
from

(select *,rank() over (partition by village_id order by as_on_date desc,household_id desc) as row_id

from prwss_main.mst_village_households mp

where as_on_date <= to_date($P{to_date},'yyyy-MM-dd')
and mp.status<>'D'
order by village_id) main

where row_id=1

) x

group by village_id

)y

on y.village_id=svd.village_id,prwss_main.mst_district d,prwss_main.mst_zone z,prwss_main.mst_circle c,prwss_main.mst_village mv,prwss_main.mst_divisional di

where svd.district_id=d.district_id
---and d.district_id = di.district_id
and d.circle_id=c.circle_id

and c.zone_id=z.zone_id

and mv.village_id=svd.village_id


order by zone_name,circle_name,district_name
		) main
		left join
		(select divisional_id,divisional_name,district_id from prwss_main.mst_divisional)di
		on di.divisional_id = main.location_id)sds
		left join
		(select village,no_of_water_connection_after_up_to_pre as pre_mnth_conn,
case when no_of_water_connection_after_during>0 then (no_of_water_connection_after_during-no_of_water_connection_after_up_to_pre) else 0 end as during_mnth_conn from(
select village,no_of_water_connection_after_up_to_pre, case when no_of_water_connection_after_during>0 then no_of_water_connection_after_during else 0 end as no_of_water_connection_after_during
from(select distinct main.village_id as village,sew_connection as no_of_water_connection_after_up_to_pre from
(select mp.*,rank() over (partition by mp.village_id order by as_on_date desc,sewerage_tariff_id desc) as row_id
from prwss_main.mst_village_sewerage_tariff mp left outer join prwss_main.mst_scheme_village msv on mp.village_id=msv.village_id
where as_on_date <  date_trunc('month', to_date($P{to_date},'yyyy-MM-dd'))
and msv.scheme_commissioned_date <= as_on_date
and mp.status<>'D'and msv.status<>'D'
order by mp.village_id) main where row_id=1)aa
left join
(select distinct main.village_id,sew_connection as no_of_water_connection_after_during from
(select mp.*,rank() over (partition by mp.village_id order by as_on_date desc,sewerage_tariff_id desc) as row_id
from prwss_main.mst_village_sewerage_tariff mp left outer join prwss_main.mst_scheme_village msv on mp.village_id=msv.village_id
where as_on_date <to_date($P{to_date},'yyyy-MM-dd') and
as_on_date>=date_trunc('month', to_date($P{to_date},'yyyy-MM-dd'))
and msv.scheme_commissioned_date <= as_on_date and mp.status<>'D' and msv.status<>'D'
order by mp.village_id) main where row_id=1)bb
on aa.village =bb.village_id)pp)ppp
		on sds.village_id=ppp.village

		where $P!{where}
order by zone_name,circle_name,district_name,divisional_name,village_name asc]]>
