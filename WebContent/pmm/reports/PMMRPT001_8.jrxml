	<queryString>
		<![CDATA[select  zone_id,zone_name,circle_id,circle_name,district_id,district_name,
	nc_pc_mh,nc_pc_oh,total_nc_pc,wb_mh,wb_oh,wb_total,
	case when nc_pc_mh<>0 then (wb_mh*100/nc_pc_mh) else 0 end  as swap_per_mh,
	case when nc_pc_oh<>0 then (wb_oh*100/nc_pc_oh) else 0 end  as swap_per_oh,
	case when total_nc_pc<>0 then (wb_total*100/total_nc_pc) else 0 end  as swap__per_total,
	total_habitation_to_be_covered as total_per_impr,fc_upgraded as participating_habi,
	case when total_habitation_to_be_covered<>0 then (fc_upgraded*100/total_habitation_to_be_covered) else 0 end  as habi_planned_to_be,
	case when loc_villages<>0 then (loc_villages*100/comm_villages) else 0 end  as per_resource_disbursed
from
(select z.zone_id,zone_name,c.circle_id,circle_name,d.district_id,district_name,total_habitation_to_be_covered,
	sum(case when nc_pc_status in ('NC','PC') and habitation_type='MH' then 1 else 0 end )as nc_pc_mh,
	sum(case when nc_pc_status in ('NC','PC') and habitation_type='OH' then 1 else 0 end )as nc_pc_oh,
	sum(case when nc_pc_status in ('NC','PC') and habitation_type in ('MH','OH') then 1 else 0 end )as total_nc_pc,


	sum(case when nc_pc_status in ('NC','PC','FC') and scheme_id not like '%IMP%' and scheme_id not like '%SEW%'  and scheme_commissioned_date>='2008-04-01' and rank=1 and habitation_type='MH' then 1 else 0 end )as wb_mh,
	sum(case when nc_pc_status in ('NC','PC','FC') and scheme_id not like '%IMP%' and scheme_id not like '%SEW%'  and scheme_commissioned_date>='2008-04-01' and rank=1 and habitation_type='OH' then 1 else 0 end )as wb_oh,
	sum(case when nc_pc_status in ('NC','PC','FC') and scheme_id not like '%IMP%' and scheme_id not like '%SEW%'  and scheme_commissioned_date>='2008-04-01' and rank=1 and habitation_type in ('MH','OH') then 1 else 0 end )as wb_total,

	sum(case when nc_pc_status ='FC' and habitation_type in ('MH','OH') then 1 else 0 end )as total_fc_habitation,
	sum(case when nc_pc_status in ('NC','PC','FC') and habitation_type in ('MH','OH') and scheme_id like '%IMP%' and prog_id in ('PROG15') and scheme_commissioned_date >='2008-04-01' then 1 else 0 end )as fc_upgraded,

	sum(case when scheme_commissioned_date is not null and rank=1 then 1 else 0 end )as comm_villages,
	sum(case when loc_scheme is not null and loc_rank=1 then 1 else 0 end )as loc_villages



from
(select divisional_office_id,loc_scheme,scheme_id,prog_id,scheme_commissioned_date,mv.village_id,habitation_type,rank,
	rank() over (partition by mv.village_id order by loc_scheme) as loc_rank
from prwss_main.mst_village mv
left join
(select sh.scheme_id,prog_id,scheme_commissioned_date,village_id,location_id,
	rank() over (partition by village_id order by scheme_commissioned_date desc,sh.scheme_upgraded)rank
	from prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd
	where sv.scheme_id=sh.scheme_id and sh.scheme_id=sd.scheme_id and sv.status<>'D' and sh.status<>'D' and sd.status<>'D'
	--and   village_id in (select village_id from prwss_main.vw_prwss_project_habitation)
	and   sh.scheme_source<>'HANDPUMP'
	and   sh.scheme_source<>'SEWERAGE'
	and   sh.prog_id = 'PROG15'
	and   sh.scheme_upgraded = 'NO'
	and   sd.scheme_upgraded = 'NO'
	and   sv.scheme_upgraded = 'NO'
	and   scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd')) svh
on mv.village_id=svh.village_id
--and  mv.divisional_office_id=svh.location_id
and rank=1
left join
(select distinct scheme_id as loc_scheme from prwss_main.t_finance_loc_detail where status<>'D' )ld
on svh.scheme_id=ld.loc_scheme
where mv.status<>'D' ) xx,prwss_main.tbl_proposed_program_2008 ncpc,
(select district_id, count(distinct scheme_id) as total_habitation_to_be_covered from
(select distinct md.district_id,scheme_id from
(select distinct sh.scheme_id,sh.scheme_name, sh.location_id,sh.district_id from
prwss_main.t_proc_sub_plan_scheme sps, prwss_main.mst_scheme_header sh
where sps.scheme_id = sh.scheme_id
and  sh.status <> 'D' and sps.status <> 'D' and (sps.scheme_id like '%IMP%'))x
right join prwss_main.mst_district md
on md.district_id = x.district_id ) x2
group by district_id) imp_target,
prwss_main.mst_divisional di,prwss_main.mst_district d,prwss_main.mst_circle c,prwss_main.mst_zone z
where xx.village_id=ncpc.village_id
and   xx.divisional_office_id=di.divisional_id
and   di.district_id=d.district_id
and   d.district_id=imp_target.district_id
and   d.circle_id=c.circle_id
and   c.zone_id=z.zone_id
group by  z.zone_id,zone_name,c.circle_id,circle_name,d.district_id,district_name,total_habitation_to_be_covered) main
where $P!{where}
order by district_name]]>
	</queryString>
