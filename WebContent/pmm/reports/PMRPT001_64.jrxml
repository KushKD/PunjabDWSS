	<queryString>
		<![CDATA[SELECT ZONE_ID,ZONE_NAME,CIRCLE_ID,CIRCLE_NAME,DISTRICT_ID,DISTRICT_NAME,COUNT(DISTINCT VILLAGE_ID)VILLAGE,SUM(GC_HOUSEHOLDS) GC_HOUSEHOLDS,SUM(SC_HOUSEHOLDS) SC_HOUSEHOLDS FROM (
SELECT Z.ZONE_ID,ZONE_NAME,C.CIRCLE_ID,CIRCLE_NAME,D.DISTRICT_ID,DISTRICT_NAME,B.BLOCK_ID,BLOCK_NAME,VILLAGE_ID,VILLAGE_NAME,gc_households,sc_households  FROM(SELECT * FROM(SELECT VILLAGE_ID,gc_households,sc_households FROM prwss_main.mst_village_households
WHERE ent_date>'2015-08-01' AND STATUS<>'D')AA
LEFT JOIN(
SELECT VILLAGE_ID AS VILLAGE,VILLAGE_NAME,DISTRICT_ID,BLOCK_ID,divisional_office_id FROM prwss_main.mst_village WHERE STATUS<>'D')BB
ON AA.VILLAGE_ID =BB.VILLAGE)MAIN,prwss_main.mst_block B,prwss_main.mst_district D,prwss_main.mst_circle C,prwss_main.mst_ZONE Z,prwss_main.mst_divisional DI
WHERE MAIN.BLOCK_ID =B.BLOCK_ID
AND DI.DIVISIONAL_ID=MAIN.divisional_office_id
AND MAIN.DISTRICT_ID=D.DISTRICT_ID
AND D.CIRCLE_ID=C.CIRCLE_ID  AND C.ZONE_ID =Z.ZONE_ID)MAIN
GROUP BY ZONE_ID,ZONE_NAME,CIRCLE_ID,CIRCLE_NAME,DISTRICT_ID,DISTRICT_NAME
ORDER BY ZONE_NAME,CIRCLE_NAME,DISTRICT_NAME]]>
	</queryString>
