	<queryString>
		<![CDATA[select * from(select *,prwss_main.get_location_name(district_id) as district_name , prwss_main.get_location_name(division_id)as division_name, prwss_main.get_location_name(block_id) as block_name,prwss_main.get_location_name(aa.village_id) village_name from
   (SELECT sc.sample_id, sample_number, scheme_id, water_source, scheme_status,
       sc.district_id, sc.division_id, sc.tehsil_id, sc.village_id, sc.block_id, gram_panchayat_id,
       habitation_type, lab_to_be_tested,lb.lab_id, lab_name, lb.village_id as lb_village_id,tr.test_result_id as test_result_id1, tr.sample_part_no, tr.sample_id, tr.lab_id
       --,trd.test_result_detail_id, trd.test_result_id,  trd.param_id, trd.actual_value,pr.parameter_id, pr.parameter_name, pr.uom, pr.permissible_limit, pr.acceptable_limit
  FROM prwss_main.wq_sample_collection sc ,prwss_main.wq_lab_master lb,prwss_main.wq_test_result tr--, prwss_main.wq_test_result_dtl trd -- prwss_main.wq_parameter_master pr
    where lb.lab_id=sc.lab_to_be_tested and tr.sample_id=sc.sample_id --and trd.test_result_id=tr.test_result_id --and trd.param_id=pr.parameter_id
  and sc.active_flag =1)aa, (SELECT * FROM   crosstab(
   'SELECT test_result_id, param_id, actual_value
    FROM   prwss_main.wq_test_result_dtl
    ORDER  BY 1,2'
   ) AS actual_value ("test_result_id" int, "Colour"character varying,"Odour"character varying,"Taste"character varying,"Turbidity"character varying,"pH_value"character varying,
"Total_Dissolved_Solids"character varying,"Total_Hardnes"character varying,"Iron"character varying,"Chloride"character varying,"Free_Residual_Chlorine"character varying,
"Sulphate"character varying,"Sulphide"character varying,"Nitrate"character varying,"Fluoride"character varying,"Calcium"character varying,"Copper"character varying,"Magnesium"character varying,
"Manganese"character varying,"Mercury"character varying,"Cadmium"character varying,"Selenium"character varying,"Total_Arsenic"character varying,"Cyanide"character varying,
"Lead"character varying,"Zinc"character varying,"Total_Chromium"character varying,"Aluminium"character varying,"Boron"character varying,"Ammonia"character varying,
"Barium"character varying,"Silver"character varying,"Molybdenum"character varying,"Nickel"character varying,"Phenolic_Compound"character varying,
"Polychlorinated_biphenyls"character varying,"Polynuclear aromatic_Hydrocarbons"character varying,"Anionic_Detergents"character varying,
"Chloramines"character varying,"Bromoform"character varying,"Dibromochloromethane"character varying,"Bromodichloromethane"character varying,
"Chloroform"character varying,"Alpha emitters"character varying,"Beta emitters"character varying,"Uranium"character varying,
"E_Coli_or_thermotolerant_coliform_bacteria"character varying,"Total_Coliform_bacteria"character varying,
"Mineral_Oil"character varying,"Pesticides"character varying,"Total_Alkalinity"character varying))bb
   where aa.test_result_id1=bb.test_result_id )zz]]>
