	<queryString>
		<![CDATA[----rrrohjit
 select * from( select * from(
select DISTINCT main2.*,z.zone_id,z.zone_name,c.circle_id,c.circle_name,d.district_id,d.district_name,di.divisional_name,di.divisional_id,
case when cnt_scheme is not null then cnt_scheme else cnt_scheme1 end as cnt_scheme2
,case when scheme_id is not null then scheme_id else scheme end as final_scheme
,case when package_id is not null then package_id  else 'Package not done' end as package_id1
,case when contract_status1 is null then 'New' else contract_status1 end as contract_status
,case when post_prior_status1 is not null then post_prior_status1 else 'POST-REVIEW' end as post_prior_status
,case when program_id1 is not null then program_id1 else prog_id end as program_id


 from(select * ,case WHEN main.loct_id is not null then main.loct_id else vc.location_id end as loc_id from(
select   *,case when a_p='A' and estimate_package_cost is not null then round(estimate_package_cost/100000,2) else 0 end as estimate_pack_cost_a,
	case when a_p='A' and dsr_sanction_amount is not null then dsr_sanction_amount else 0 end as dsr_sanction_amount_a,
	case when a_p='A' and expenditure is not null then cast(expenditure as numeric) else 0 end as expenditure_a,
	case when a_p='A' and package_id is not null then 1 else 0 end as pkg_count_a

from (select distinct location_id as loct_id,package_description,scheme_id,cnt_scheme,program_id as program_id1 ,
	package_id,estimate_package_cost,method_procurement,design_investigation_date,
		dsr_sanction_date,dsr_sanction_amount,est_prepared_sanction_date,prep_bid_doc_date,bank_noc_bid_date,bid_invitation_date,bid_opening_date,
		con_award_decide_date,bank_noc_con_award_date,con_sign_date,contract_id,vendor,wb_number,contract_end_date,a_p, post_prior_status as post_prior_status1
		,scheme_source,expenditure,psr_estimate_cost1,contract_status as contract_status1
	from ( select distinct xyz.*,case when contract_status is null then 'New' else contract_status end as contract_status
	from
	(select distinct  c.location_id,package_description,sh.scheme_id,(scheme_name||' ('||sh.scheme_id||')') as cnt_scheme,program_id ,a.package_id,
		(round(scheme_estimated_cost/100000,2)||'/'||round(admin_app_gop_amount/100000,2)) as psr_estimate_cost1,
		round(estimate_package_cost,2)estimate_package_cost,method_procurement,
case when method_procurement='NCB' then to_char(b.design_investigation_date,'dd-MON-yyyy') else 'N/A' end as design_investigation_date,
		to_char(est_prepared_sanction_date,'dd-MON-yyyy') as dsr_sanction_date,0 as dsr_sanction_amount,
	est_prepared_sanction_date,prep_bid_doc_date,
	case when post_prior_status='PRIOR-REVIEW' then to_char(bank_noc_bid_date ,'dd-mm-yyyy') else 'POST-REVIEW' end as bank_noc_bid_date,bid_invitation_date,bid_opening_date,
case when method_procurement='NCB' then to_char(con_award_decide_date,'dd-MON-yyyy') else 'N/A' end as con_award_decide_date,
	case when post_prior_status='PRIOR-REVIEW' then to_char(bank_noc_con_award_date ,'dd-mm-yyyy') else 'POST-REVIEW' end as bank_noc_con_award_date,
	 (to_char(con_sign_date,'DD-MON-YYYY')) as con_sign_date,
	'N/A' contract_id,'N/A' vendor,
	'N/A' as wb_number,con_completion contract_end_date,'N/A' expenditure , 'P' as a_p,a.post_prior_status,sh.ws_sw  as scheme_source
	from prwss_main.t_proc_sub_plan_scheme ss,prwss_main.t_proc_package_header a,prwss_main.t_proc_package_works b, prwss_main.t_proc_plan c,prwss_main.mst_scheme_header sh,
	     (SELECT * FROM(select *,RANK () OVER (PARTITION BY SCHEME_ID ORDER BY admin_app_gop_letter_date DESC)
	from prwss_main.mst_scheme_details )AA WHERE RANK =1 ) sd
	where ss.scheme_id=a.scheme_id
	and   ss.scheme_id=sh.scheme_id
	and   sh.scheme_id=sd.scheme_id
	and   a.plan_id=c.plan_id
	and   a.sub_plan_id=ss.sub_plan_id
	and   a.package_id=b.package_id
	and   package_type='WORKS' and a.status<>'D' and b.status<>'D' and c.status<>'D' and ss.status<>'D' and sh.status<>'D' and sd.status<>'D')xyz
	left join
	(select package_id,case when publish_date is null and contract_status is null then 'New' else contract_status end as contract_status
		from
		(select distinct package_id,rank_c,publish_date,case when actual_completion_date is not null and actual_completion_date<=now() then 'Completed'
				       when (actual_completion_date is  null or actual_completion_date>now()) or (publish_date is not null and tender_aw is null) then 'Ongoing' end as contract_status
		from
		(select distinct package_id,th.tender_id,ta.tender_id as tender_aw,publish_date,signing_of_contract,contract_no,notice_to_proceed,tender_amount,
			rank() over (partition by package_id order by publish_date desc) as rank_c
			from prwss_main.t_tender_hdr th left join (select * from prwss_main.t_tender_award where status<>'D')ta
			on th.tender_id=ta.tender_id where th.status<>'D' )ten
		left join
		(select ta.tender_id,actual_completion_date,contract_id
			from prwss_main.t_tender_award ta
			left join
			(select tender_id,actual_completion_date,contract_id
			from prwss_main.t_contract_mgmt_hdr ch,prwss_main.mst_vendor v
			where ch.vendor_id=v.vendor_id and ch.status<>'D' and v.status<>'D')chv
			on ta.tender_id=chv.tender_id
			and ta.contract_no=chv.contract_id)con
		on ten.tender_id=con.tender_id where  rank_c=1)xx )abc
	on xyz.package_id=abc.package_id
	$P!{innerWhere}

union all


select location_id,'' as package_description,scheme_id,cnt_scheme,program_id,package_id,null as psr_estimate_cost1,null as estimate_package_cost,null as method_procurement
,null as design_investigation_date ,null as  dsr_sanction_date,null as dsr_sanction_amount
,null as est_prepared_sanction_date,null as prep_bid_doc_date,null as bank_noc_bid_date,null as  bid_invitation_date,null as bid_opening_date,null as con_award_decide_date
,null as bank_noc_con_award_date,null as con_sign_date,null contract_id,null as vendor ,null as wb_number ,null as contract_end_date,null as expenditure,
null as a_p,'POST_REVIEW' as post_prior_status ,null as  scheme_source ,'New'  as contract_status
from(
SELECT location_id,(scheme_name||' ('||scheme_id||')') as cnt_scheme ,scheme_id,plan_id as plan,program_id , 'Package Not Defined' as package_id  FROM(
select location_id,AA.SCHEME_ID,scheme_name,AA.SUB_PLAN_ID,AA.PLAN_ID,PROGRAM_ID,BB.PACKAGE_ID as pack
 from(select distinct sh.location_id, sh.scheme_id,scheme_name,xx.plan_id,XX.SUB_PLAN_ID,PROGRAM_ID
	from prwss_main.t_proc_sub_plan_scheme ss, prwss_main.t_proc_plan c,prwss_main.mst_scheme_header sh,
	prwss_main.t_proc_sub_plan xx,	     (SELECT * FROM(select *,RANK () OVER (PARTITION BY SCHEME_ID ORDER BY admin_app_gop_letter_date DESC)
	from prwss_main.mst_scheme_details )AA WHERE RANK =1 ) sd--,prwss_main.t_proc_package_header
	where sh.scheme_id=sd.scheme_id and   ss.scheme_id=sh.scheme_id
	and ss.sub_plan_id=xx.sub_plan_id and 	  xx.plan_id=c.plan_id
	and c.status<>'D' and ss.status<>'D' and sh.status<>'D' and sd.status<>'D' --and sh.scheme_id='TW1382'
	 )aa
	LEFT OUTER JOIN
		(select scheme_id,SUB_PLAN_ID,package_ID from prwss_main.t_proc_package_header WHERE STATUS<>'D')BB ON AA.SUB_PLAN_ID= BB.SUB_PLAN_ID )ZZ
		WHERE PACK IS NULL )aa
	union all
	select xx.location_id,package_description,scheme_code as scheme_id,(scheme_name||' ('||scheme_code||')') as cnt_scheme,program_id,xx.package_id,null psr_estimate_cost,round(estimate_package_cost,2)estimate_package_cost,
	method_procurement,case when method_procurement='NCB' then to_char(sd.design_investigation_date,'dd-MON-yyyy')  else 'N/A' end as design_investigation_date,

	case when tech_app_ce_letter_date is not null then to_char(tech_app_ce_letter_date,'dd-MON-yyyy')
	   else (case when tech_app_se_letter_date is not null  then to_char(tech_app_se_letter_date,'dd-MON-yyyy')
	    else (case when tech_app_ee_date is not null then to_char(tech_app_ee_date,'dd-MON-yyyy') else '' end) end) end  as dsr_sanction_date,
	round((case when tech_app_ce_amount<>0 then tech_app_ce_amount
	  else (case when tech_app_se_amount<>0 then tech_app_se_amount
	  else tech_app_ee_amount end) end )/100000,2) as dsr_sanction_amount,
	xx.est_prepared_sanction_date,xx.prep_bid_doc_date,
	case when post_prior_status='PRIOR-REVIEW' then to_char(xx.wb_bid_doc_noc_date ,'dd-mm-yyyy') else 'POST-REVIEW' end as bank_noc_bid_date,xx.bid_invitation_date,xx.bid_opening_date,
case when method_procurement='NCB' then to_char(con_award_decide_date,'dd-MON-yyyy') else 'N/A' end as con_award_decide_date,
	case when post_prior_status='PRIOR-REVIEW' then to_char(xx.wb_contract_award_noc_date ,'dd-mm-yyyy') else 'POST-REVIEW' end as bank_noc_con_award_date,
	xx.con_sign_date,xx.contract_id,xx.vendor,
	case when post_prior_status='PRIOR-REVIEW' then pd.wb_number else 'POST-REVIEW' end as wb_number,xx.contract_end_date,cast((sum(round(COALESCE(xx.expenditure,0)/100000,2))) as character varying)expenditure,
	'A' as a_p, post_prior_status,sh.ws_sw as scheme_source,
	case when contract_status is null then 'New' else contract_status end as contract_status
from
	(select ten_con.*,rank() over (partition by package_id order by bid_invitation_date desc) as rank_a
	from
	(select * from
	(select th.location_id,th.tender_id,th.package_description,scheme_code,th.package_id,estimated_tender_amount as estimate_package_cost,
		procurement_type as method_procurement,th.est_prepared_sanction_date,th.bid_sanction_date as prep_bid_doc_date,th.bank_noc_bid_date, bank_noc_final_contract_date,
		th.publish_date bid_invitation_date,th.open_date bid_opening_date,a.wb_bid_doc_noc_date , a.wb_contract_award_noc_date,
		a.post_prior_status

	from prwss_main.t_tender_hdr th,prwss_main.t_tender_detail td,prwss_main.t_proc_package_header a
	where th.tender_id=td.tender_id
	and th.package_id=a.package_id
	and th.status<>'D'
	and tender_activity='W7' and tender_type='WORKS'
	) ten
	left join
	(select * from
		(
			select distinct case when publish_date is null and contract_status is null then 'New' else contract_status end as contract_status,
				case when tender_amount is null then (to_char(signing_of_contract,'DD-MON-YYYY'))||'/INR'
				     when tender_amount is not null then (to_char(signing_of_contract,'DD-MON-YYYY'))||' /'||round(tender_amount/100000,2)||' /INR'
				end as con_sign_date,contract_id,vendor_name||' / '||pin||' / Indian' as vendor,contract_end_date,
				tender_id,con_award_decide_date
			from
			(select distinct package_id,publish_date,signing_of_contract,vendor_name,pin,contract_end_date,ten.tender_id,
				case when actual_completion_date is not null and actual_completion_date<=now() then 'Completed'
				     when (actual_completion_date is  null or actual_completion_date>now()) or (publish_date is not null and tender_aw is null) then 'Ongoing' end as contract_status,contract_no as contract_id,
				notice_to_proceed as con_award_decide_date,tender_amount,rank_b
			from
			(select distinct package_id,th.tender_id,ta.tender_id tender_aw,publish_date,signing_of_contract,contract_no,notice_to_proceed,tender_amount,
				rank() over (partition by package_id order by publish_date desc) as rank_b
			from prwss_main.t_tender_hdr th left join (select * from prwss_main.t_tender_award where status<>'D') ta
			on th.tender_id=ta.tender_id where th.status<>'D')ten
			left join
			(select ta.tender_id,actual_completion_date,contract_id,vendor_name,pin,ta.contract_end_date as contract_end_date
				from prwss_main.t_tender_award ta
				left join
				(select tender_id,actual_completion_date,contract_id,vendor_name,pin
				from prwss_main.t_contract_mgmt_hdr ch,prwss_main.mst_vendor v
				where ch.vendor_id=v.vendor_id and ch.status<>'D' and v.status<>'D')chv
				on ta.tender_id=chv.tender_id and ta.contract_no=chv.contract_id
			)con
			on ten.tender_id=con.tender_id where rank_b=1)xx

		)y
		left outer join
		(
			select contract_id dtl_contract,sum(release_amount) as expenditure
			from prwss_main.t_contract_mgmt_detail cd
			where status<>'D'
			group by contract_id
		)z
		on y.contract_id=z.dtl_contract
	) con
	on ten.tender_id = con.tender_id) ten_con
	left join
	(select location_id,scheme_id,sum(payment_amount) as expend from prwss_main.t_finance_gpwsc_register where status<>'D' and transaction_type='PAYMENT'
	group by location_id,scheme_id) ex
	on ten_con.scheme_code=ex.scheme_id
	and ten_con.location_id=ex.location_id
	) xx,prwss_main.mst_scheme_header sh,(SELECT * FROM(select *,RANK () OVER (PARTITION BY SCHEME_ID ORDER BY admin_app_gop_letter_date DESC)
	from prwss_main.mst_scheme_details )AA WHERE RANK =1 ) sd,
						(select package_id,plan_from,plan_to,wb_number,PROGRAM_ID from prwss_main.t_proc_package_header ph,prwss_main.t_proc_plan pp
						 where  ph.plan_id=pp.plan_id and ph.status<>'D' and pp.status<>'D') pd
	where xx.scheme_code=sh.scheme_id
	and   sh.scheme_id=sd.scheme_id
	and   xx.package_id=pd.package_id
	and   rank_a=1
	and sh.status<>'D' and sd.status<>'D'
	$P!{innerWhere}
	group by xx.location_id,package_description,scheme_code,cnt_scheme,program_id,xx.package_id,estimate_package_cost,method_procurement,sd.design_investigation_date,
		dsr_sanction_date,dsr_sanction_amount,xx.est_prepared_sanction_date,xx.prep_bid_doc_date,bank_noc_bid_date,xx.bid_invitation_date,xx.bid_opening_date,
		con_award_decide_date,bank_noc_con_award_date,xx.con_sign_date,xx.contract_id,xx.vendor,wb_number,xx.contract_end_date,a_p, post_prior_status,contract_status,
		sh.ws_sw, wb_bid_doc_noc_date, wb_contract_award_noc_date
	) m
	)main1
	--where $P!{where}
	order by cnt_scheme,package_id,a_p desc)Main
	----------------Village_category---
	full outer join
	(select * FROM(select * from(select (scheme_name||' ('||sh.scheme_id||')') as cnt_scheme1,	sd.SCHEME_ID as scheme,ADMIN_APP_GOP_LETTER_DATE,sd.STATUS
 ,scheme_name,prog_id,location_id ,scheme_commissioned_date,village_category,sv.SCHEME_UPGRADED
 ,rank () over(partition by sv.scheme_id order by scheme_commissioned_date desc,admin_app_gop_letter_date desc)
from prwss_main.mst_scheme_details SD, prwss_main.mst_scheme_header SH,prwss_main.mst_scheme_village SV
where 	sh.scheme_id=sd.scheme_id 	and   sd.scheme_id=sv.scheme_id
and sh.SCHEME_UPGRADED=sd.SCHEME_UPGRADED 	and   sd.SCHEME_UPGRADED=sv.SCHEME_UPGRADED
and sh.status<>'D' and sv.status<>'D' and sd.status<>'D')AA where rank=1 )BB --where prog_id='PROG30' and village_category is not null
)vc on main.scheme_id=vc.scheme)main2 , prwss_main.mst_zone z,prwss_main.mst_circle c,prwss_main.mst_district d,prwss_main.mst_divisional di
	where z.zone_id=c.zone_id
	and c.circle_id=d.circle_id
	and d.district_id=di.district_id
	and main2.loc_id=di.divisional_id)XX
	left outer join
	(select distinct (sch_name||' ('||scheme_id||')') scheme,(round(scheme_estimated_cost/100000,2)||'/'||round(admin_app_gop_amount/100000,2)) as psr_estimate_cost
from(select  sd.scheme_id,sch_name,admin_app_gop_amount, scheme_estimated_cost,rank () over (partition by sd.scheme_id order by admin_app_gop_letter_date desc) rr1
,rank () over (partition by ps.scheme_id order by ps.auth_date desc)rr2
from  prwss_main.mst_scheme_details sd, prwss_main.t_proc_sub_plan_scheme ps where sd.status<>'D' and ps.status<>'D'
and sd.scheme_id=ps.scheme_id)aa where rr1=1 and rr2=1)ZZ on xx.cnt_scheme2=zz.scheme)alll

	where  $P!{where}
 --where PROG_ID='PROG30' AND village_category ='2B'
order by zone_name,circle_name,district_name,cnt_scheme2,package_id,a_p desc]]>
	</queryString>
