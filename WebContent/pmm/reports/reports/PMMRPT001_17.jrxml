	<queryString>
		<![CDATA[select distinct *
from
(select mz.zone_id,zone_name,mc.circle_id,circle_name,md.district_id,district_name,block_name,village_name,xxx.*
--,rank() over (partition by mv.village_id order by assessment_date desc) rank
from
(select a.village_id,a.scheme_id,(scheme_name||' ('||a.scheme_id||')') as scheme_name,program_id,
scheme_source,households,no_of_water_connection,assessment_date,scheme_commissioned_date ,scheme_operated_by,swap_nonswap,
	Characterstics,
case when Characterstics is null then 'Not Entered' else Characterstics end Characterstic,
	present_score,pre_score

from(select * from
(select sh.scheme_id,scheme_name,mv.village_id,sh.scheme_source,scheme_commissioned_date,sv.scheme_operated_by,prog_id as program_id,
	case when prog_id='PROG15' then 'SWAP-IDA'
                     when prog_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'  end as swap_nonswap,
	rank() over (partition by mv.village_id order by scheme_commissioned_date desc,sh.scheme_id desc) rank
from prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd,prwss_main.mst_village mv
where sh.scheme_id=sv.scheme_id
and   sv.scheme_id=sd.scheme_id
and   sv.village_id=mv.village_id
and   habitation_type='MH'
and	  sv.scheme_upgraded not like 'ext%'
and	  sd.scheme_upgraded not like 'ext%'
and	  sh.scheme_upgraded not like 'ext%'
and   scheme_commissioned_date between to_date('2008-04-01','yyyy-mm-dd') and  to_date('$P!{to_date}','yyyy-mm-dd')
and   sh.scheme_source<>'HANDPUMP' and sh.scheme_source <> 'SEWERAGE' and sh.scheme_source <> 'IMPROVEMENT'

and 	scheme_operated_by like '%GP%'

and   sh.status<>'D' and sv.status<>'D' and sd.status<>'D' and mv.status<>'D') x where rank=1 and	scheme_commissioned_date < CURRENT_TIMESTAMP - INTERVAL '365 days'
and swap_nonswap is not null) a
left join
(select * from (select village_id,(gc_households+sc_households) as households,
	rank() over (partition by village_id order by as_on_date desc,household_id desc) rank
from prwss_main.mst_village_households where status<>'D')x where rank=1) mp
on a.village_id=mp.village_id
left join
(select * from
(select village_id,no_of_water_connection,
	rank() over (partition by village_id order by as_on_date desc,connection_id desc) rank
from prwss_main.mst_village_connection where status<>'D')x where rank=1) mc
on a.village_id=mc.village_id
left outer join
(select distinct  x.village_id,x.present_score,xx.pre_score,x.Characterstics,x.assessment_date
from
(select  distinct village_id,case when rank=1 then mrks else null end as present_score,
	case when rank=2 then mrks else null end as pre_score,
	case when revenue_collected_morethanomexpenditure >= 9 then 'SUSTAINABLE'
	when revenue_collected_morethanomexpenditure <9  then 'NON-SUSTAINABLE'
	when mrks =0 or mrks is null then 'SCORES NOT-ENTERED' end  as Characterstics,rank,assessment_date
from
(select x.*,rank() over (partition by village_id order by assessment_date desc) rank
from
(select village_id,revenue_collected_morethanomexpenditure,(sourcefail_summer +  disinfection_daily +  source_remained_potable +  proper_protection_arrangement +  year_inclusive_maintenance_shut +  supply_water_less_than50percent +
	total_household_pittaps +  supply_water_less_than120percent +  electric_consumgpwsc +  revenue_collected_morethan50percent +  revenue_collected_morethan90percent +
	revenue_collected_morethanomexpenditure +  prepared_annual_omplan +  annual_meetings_held +  prepared_approvedsoa +  women_morethan33percent) mrks,assessment_date
	from  prwss_main.pmm_sust_assessment where status = 'A')x)xx
where rank in (1))x
left join
(select  distinct village_id,case when rank=1 then mrks else null end as present_score,
	case when rank=2 then mrks else null end as pre_score,
	case when revenue_collected_morethanomexpenditure >= 9 then 'SUSTAINABLE'
	when revenue_collected_morethanomexpenditure <9  then 'NON-SUSTAINABLE'
	when mrks =0 or mrks is null then 'SCORES NOT-ENTERED' end  as Characterstics,rank
from
(select x.*,rank() over (partition by village_id order by assessment_date desc) rank
from
(select village_id,revenue_collected_morethanomexpenditure,(sourcefail_summer +  disinfection_daily +  source_remained_potable +  proper_protection_arrangement +  year_inclusive_maintenance_shut +  supply_water_less_than50percent +
	total_household_pittaps +  supply_water_less_than120percent +  electric_consumgpwsc +  revenue_collected_morethan50percent +  revenue_collected_morethan90percent +
	revenue_collected_morethanomexpenditure +  prepared_annual_omplan +  annual_meetings_held +  prepared_approvedsoa +  women_morethan33percent) mrks,assessment_date
	from  prwss_main.pmm_sust_assessment where status = 'A')x)xx
where rank in (2))xx
on x.village_id=xx.village_id
) sa
on a.village_id=sa.village_id
) xxx,prwss_main.mst_village mv,prwss_main.mst_block mb,prwss_main.mst_district md,prwss_main.mst_circle mc,prwss_main.mst_zone mz
where xxx.village_id=mv.village_id
and   mv.block_id=mb.block_id
and   mb.district_id=md.district_id
and   md.circle_id=mc.circle_id
and   mc.zone_id=mz.zone_id) main
where $P!{where}
order by district_name,characterstics desc]]>
	</queryString>
