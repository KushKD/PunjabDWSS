	<parameter name="where" class="java.lang.String">
		<defaultValueExpression><![CDATA[" 1=1 "]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select * from (
select z.zone_id as zone_id,z.zone_name as zone,c.circle_id as circle_id,c.circle_name as circle,d.district_name,com.district_id,b.block_name ,com.block_id ,p.program_name,com.program_id,
	case when month_of_commissioning between '2008-04-01' and '2009-03-31' then 'Financial Year (2008-2009)'
	when month_of_commissioning between '2009-04-01' and '2010-03-31' then 'Financial Year (2009-2010)'
	when month_of_commissioning between '2010-04-01' and '2011-03-31' then 'Financial Year (2010-2011)'
	else 'Warning: Not in range, Please varify the month of commissioning.' end as fin_year,
	count(distinct com.scheme_id) sch_cnt,
	sum(case when p.swap_nonswap='SWAP' then 1 else 0 end) swap ,
	sum(case when p.swap_nonswap='NONSWAP' then 1 else 0 end) non_swap,

	sum(case when upper(com.water_works_existing_new)='NEW' then 1 when upper(com.water_works_existing_new)='NEW WATER WORKS' then 1 else 0 end) new_water_works,
	sum(case when upper(com.water_works_existing_new)='EXISTING' then 1 when upper(com.water_works_existing_new)='OLD' then 1 else 0 end) existing_water_works,

       sum(case when com.village_commissioned_nc is not null or com.village_commissioned_pc is not null then 1 else 0 end) mh_habitation,
       sum(case when com.village_commissioned_oh is not null then 1 else 0 end) oh_habitation,
       sum(case when com.village_commissioned_nc is not null then 1 else 0 end) nc_habitation,
       sum(case when com.village_commissioned_pc is not null then 1 else 0 end) pc_habitation,

	sum(case when com.source_of_scheme='Canal' then 1 else 0 end) canal,
       sum(case when com.source_of_scheme='Tube Well' then 1 when com.source_of_scheme='Perculation Well' then 1
       when com.source_of_scheme='Tube Well with RO' then 1
       when com.source_of_scheme='Lifting of water from RSD Lake' then 1 else 0 end) tubewell,

       sum(case when com.source_of_scheme='Hand Pump' then 1 when com.source_of_scheme='Distribution' then 1 else 0 end) handpump,
	sum(exp_incurred_upto_31_3_2011) exp_incurred_upto_31_3_2011,
	sum(liability_as_on_1_4_2011) liability_as_on_1_4_2011,
	sum(bal_funds_req_during_2011_12) bal_funds_req_during_2011_12

from prwss_import.commissioning_data_08_09 com left outer join prwss_main.mst_program p on trim(p.program_id)=trim(com.program_id),prwss_main.mst_district d,prwss_main.mst_circle c,prwss_main.mst_zone z,prwss_main.mst_block b
WHERE z.zone_id=c.zone_id
and c.circle_id=d.circle_id
and d.district_id=trim(com.district_id)
and b.block_id=trim(com.block_id)
and com.status<>'D'
and $P!{where}
--and d.district_id='D02'
--and com.program_id='PROG15'
group by z.zone_id,z.zone_name ,c.circle_id,c.circle_name,d.district_name,com.district_id,b.block_name ,com.block_id ,p.program_name,com.program_id,
	case when month_of_commissioning between '2008-04-01' and '2009-03-31' then 'Financial Year (2008-2009)'
	when month_of_commissioning between '2009-04-01' and '2010-03-31' then 'Financial Year (2009-2010)'
	when month_of_commissioning between '2010-04-01' and '2011-03-31' then 'Financial Year (2010-2011)'
	else 'Warning: Not in range, Please varify the month of commissioning.' end,exp_incurred_upto_31_3_2011,
	liability_as_on_1_4_2011,
	bal_funds_req_during_2011_12
	order by z.zone_name,c.circle_name,d.district_name,fin_year,b.block_name) main]]>
	</queryString>
