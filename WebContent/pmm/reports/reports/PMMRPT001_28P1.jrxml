	<queryString>
		<![CDATA[select * from (
select * from (
select * from (
select mv.*,z.zone_id,z.zone_name,c.circle_id,c.circle_name,d.district_name,mcy.constituency_name,mb.block_name,dv.divisional_id,dv.divisional_name,nc_pc.nc_pc_status
from prwss_main.mst_village mv,prwss_main.tbl_proposed_program_2008 nc_pc,
prwss_main.mst_constituency mcy,prwss_main.mst_block mb,prwss_main.mst_zone z,prwss_main.mst_circle c, prwss_main.mst_district d, prwss_main.mst_divisional dv
where mv.village_id=nc_pc.village_id
and mv.constituency_id=mcy.constituency_id
and mv.block_id=mb.block_id
and z.zone_id=c.zone_id
and c.circle_id=d.circle_id
and d.district_id=mb.district_id
and mv.divisional_office_id=dv.divisional_id
and proposed_program_id='PROG15'
and mv.habitation_type='MH'
and mv.status<>'D')x
---------------------------------household ---------------------------------------------
left outer join (
select household_id,village_id,sc_households,gc_households,as_on_date from
(select *,rank() over (partition by village_id order by as_on_date desc,household_id desc) as row_id
from prwss_main.mst_village_households
where as_on_date <=now()
and status<>'D'
order by village_id) con
where row_id=1)xh on x.village_id=xh.village_id
---------------------------------Scheme ---------------------------------------------
left outer join
(
select sh.scheme_id,sv.village_id,(sh.scheme_name||' ('||sh.scheme_id||')') as scheme_name,sh.scheme_source,resolution_date,prog_id as program_id,
case when prog_id='PROG15' then 'SWAP-IDA'
     when prog_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'
     when prog_id not in ('PROG15','PROG06','PROG11','PROG20') then 'NONSWAP' end as swap_nonswap,
--round((case when tech_app_ce_amount<>0 then tech_app_ce_amount else (case when tech_app_se_amount<>0 then tech_app_se_amount else tech_app_ee_amount end) end )*0.1/100000,2) beneficiary_due
     beneficiary_by_community as beneficiary_due
from prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd,prwss_main.mst_program p,
(
select scheme_id,village_id,scheme_commissioned_date from
(select *,rank() over (partition by village_id order by scheme_commissioned_date desc,scheme_id desc) as row_id
from prwss_main.mst_scheme_village
where status<>'D'
order by village_id) con
where row_id=1
)sv
where sh.scheme_id=sd.scheme_id
and sd.scheme_id=sv.scheme_id
and sh.prog_id=p.program_id
and sh.status<>'D'
and sd.status<>'D' and sh.scheme_source<>'HANDPUMP')xcom
 on x.village_id=xcom.village_id

---------------------------------Committee---------------------------------------------

left outer join
(
select mgr.*,gr_reg.committee_id,bene_community,bene_Voluntarily,bene_non_budgetary,bene_untied_funds,bene_state_govt
from
(
select mc.committee_id,mc.village_id,case when mc.committee_type='GPWSC' then committee_constitution_date else null end gpwsc_constitution_date,
case when mc.committee_type='SLC' then committee_constitution_date else null end slc_constitution_date,
mcb.bank_name,mcb.account_number
from
( select *
from
(select *,rank() over (partition by village_id order by committee_constitution_date asc,scheme_id desc) as row_id
from prwss_main.mst_committee
where status<>'D'
order by village_id) con
where row_id=1) mc

left outer join (select * from prwss_main.mst_committee_bank  where status<>'D')mcb
on mc.committee_id=mcb.committee_id
where mc.status<>'D')mgr
left outer join
(
select scheme_id,committee_id,
round(sum(case when receipt_type='Beneficiary-Share' then receipt_amount else 0 end)/100000,2) bene_community,
round(sum(case when receipt_type='Gap-Fund-Voluntarily' then receipt_amount else 0 end)/100000,2) bene_Voluntarily,
round(sum(case when receipt_type='Gap-Fund-Non-Budgetary' then receipt_amount else 0 end)/100000,2) bene_non_budgetary,
round(sum(case when receipt_type='Gap-Fund-Untied-Funds' then receipt_amount else 0 end)/100000,2) bene_untied_funds,
round(sum(case when receipt_type='Gap-Fund-State-Govt' then receipt_amount else 0 end)/100000,2) bene_state_govt
from prwss_main.t_finance_gpwsc_register
where transaction_type='RECEIPT'
and status<>'D'
group by scheme_id,committee_id
) gr_reg
on mgr.committee_id=gr_reg.committee_id
)xgpwsc
on x.village_id=xgpwsc.village_id
)xxx where scheme_id is not null)main
where $P!{where}
order by zone_name,circle_name,district_name,divisional_name]]>
