	<queryString>
		<![CDATA[select xxxx.*
from
(select main.*,col10,col12,col13,col14,col15,col16,col17,col18,col19,col20,col21 from
(select * from
(select mz.zone_id,zone_name,mc.circle_id,circle_name,md.district_id,district_name,block_name,divisional_name,main.*
from
(select divisional_office_id,block_id,scheme_name||' ('||scheme_id||')' as sch_name,scheme_id,village_name||' ('||com.village_id||')' as vill_name,prwss_main.get_program_name(prog_id) as prog_name,
	scheme_source,total_population,sc_households,gc_households,no_of_water_connection,no_of_standpost,scheme_commissioned_date,
	case when scheme_operated_by like '%GPWSC%' then 'yes' else null end as operated_by_gpwsc,
	'Nill' as water_terrif_charges,case when tariff_rate is null then 0 else tariff_rate end as tariff_rate,swapnonswap as swap_nonswap
from
(select mv.divisional_office_id,mv.block_id,sh.scheme_id,mv.village_id,village_name,scheme_name,prog_id,sh.scheme_source,scheme_commissioned_date,scheme_operated_by,
	case when prog_id='PROG15' then 'SWAP-IDA'
	     when prog_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'
	     when prog_id not in ('PROG15','PROG06','PROG11','PROG20') then 'NONSWAP' end as swapnonswap
from prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_details sd,prwss_main.mst_village mv
where sh.scheme_id=sv.scheme_id
and   sv.scheme_id=sd.scheme_id
and   sv.village_id=mv.village_id
and   sh.scheme_source<>'HANDPUMP'
and   scheme_commissioned_date is not null and scheme_commissioned_date>='2008-04-01'
and   sh.status<>'D' and sv.status<>'D' and sd.status<>'D' and mv.status<>'D'
and   prog_id in ('PROG06','PROG11','PROG15','PROG20','PROG26'))com

left join
-----------------------------------POPULATION-----------------------------------

(
select population_id,village_id,gen_population,sc_population,st_population,(gen_population+sc_population+st_population)total_population,population_year,as_on_date
from
(select *,rank() over (partition by village_id order by as_on_date desc,population_id desc) as row_id
from prwss_main.mst_village_population mp
where as_on_date <=now()
and status<>'D'
order by village_id) pop
where row_id=1
)xp
on com.village_id=xp.village_id
-----------------------------------HOUSEHOLDS-----------------------------------
left join

(
select household_id,village_id,sc_households,gc_households,(sc_households+gc_households) as households,as_on_date from
(select *,rank() over (partition by village_id order by as_on_date desc,household_id desc) as row_id
from prwss_main.mst_village_households
where as_on_date <=now()
and status<>'D'
order by village_id) con
where row_id=1
)xh
on com.village_id=xh.village_id
------------------------------PVT. CONNECTION-----------------------------------
left join
(
select connection_id,village_id,no_of_water_connection,no_of_standpost,as_on_date from
(select *,rank() over (partition by village_id order by as_on_date desc,connection_id desc) as row_id
from prwss_main.mst_village_connection
where as_on_date <=now()
and status<>'D'
order by village_id) con
where row_id=1
)xpc
on com.village_id=xpc.village_id

left join

(
select tariff_id,village_id,tariff_rate,as_on_date
from
(select *,rank() over (partition by village_id order by as_on_date desc,tariff_id desc) as row_id
from prwss_main.mst_village_tariff
where as_on_date <=now()
and status<>'D'
order by village_id) con
where row_id=1
)xt
on com.village_id=xt.village_id) main,prwss_main.mst_block mb,prwss_main.mst_divisional di,prwss_main.mst_district md,prwss_main.mst_circle mc,prwss_main.mst_zone mz
where main.block_id=mb.block_id
and   mb.district_id=md.district_id
and   main.divisional_office_id=di.divisional_id
and   md.circle_id=mc.circle_id
and   mc.zone_id=mz.zone_id)final)main
--where $P!{where}
left join
-----------------------------------------------------------------------------------
---------------------------------------column 10--------------

(select * from
(select location_id,scheme_id,pump_quantity*pumping_discharge as col10 from prwss_main.pmm_dsr_canal where pump_select='PUMP-CHAMBER'
union all
select location_id,scheme_id,pump_quantity*pumping_discharge  as col10 from prwss_main.pmm_dsr_tubwell where pump_select='PUMP-CHAMBER')x)a
on main.scheme_id=a.scheme_id
left join
--------------------------------------column 12-----------------
(select * from
(select location_id,scheme_id,case when tubewell_depth<>0 then (tubewell_size||' ('||tubewell_depth||')') else (tubewell_size||' ('||pumping_discharge||')') end as  col12
from prwss_main.pmm_dsr_tubwell where status<>'D'
union all
select location_id,scheme_id,cast(filter_quantity as text) as col12 from prwss_main.pmm_dsr_canal where status<>'D')x)b
on main.scheme_id=b.scheme_id
--------------------------------------column 13--------------------
left join
(select * from
(select location_id,dt.scheme_id,sum(distribution_length) as col13 from prwss_main.pmm_dsr_tubwell dt,prwss_main.pmm_dsr_tubwell_distribution dtd
where dt.scheme_id=dtd.scheme_id
and   dt.status<>'D' and dtd.status<>'D'
group by location_id,dt.scheme_id

union all

select location_id,dt.scheme_id,sum(distribution_length) as col13 from prwss_main.pmm_dsr_canal dt,prwss_main.pmm_dsr_canal_distribution dtd
where dt.scheme_id=dtd.scheme_id
and   dt.status<>'D' and dtd.status<>'D'
group by location_id,dt.scheme_id)x)c
on main.scheme_id=c.scheme_id
------------------------------------column 14------------------------
left join
(select * from
(select location_id,scheme_id,rcc_capacity as col14 from prwss_main.pmm_dsr_tubwell
union all
select location_id,scheme_id,cast(rcc_capacity as character varying) as col14 from prwss_main.pmm_dsr_canal)x)d
on main.scheme_id=d.scheme_id
------------------------------------column 15--------------------------
left join
(select * from
(select location_id,scheme_id,rcc_fsl as col15 from prwss_main.pmm_dsr_tubwell
union all
select location_id,scheme_id,rcc_fsl as col15 from prwss_main.pmm_dsr_canal)x)e
on main.scheme_id=e.scheme_id
left join
----------------------------------column 16----------------------------
(select * from
(select location_id,scheme_id,disinfection_type as col16 from prwss_main.pmm_dsr_tubwell
union all
select location_id,scheme_id,disinfection_type  as col16 from prwss_main.pmm_dsr_canal)x)f
on main.scheme_id=f.scheme_id
----------------------------------column 17--------------------------
left join
(select * from
(select location_id,scheme_id,tubewell_cost as col17 from prwss_main.pmm_dsr_tubwell
union all
select location_id,scheme_id,pump_cost as col17 from prwss_main.pmm_dsr_canal)x)g
on main.scheme_id=g.scheme_id
----------------------------------column 18---------------------------
left join
(select * from
(select location_id,scheme_id,pumping_cost as col18 from prwss_main.pmm_dsr_tubwell
union all
select location_id,scheme_id,pumping_cost as col18 from prwss_main.pmm_dsr_canal)x)h
on main.scheme_id=h.scheme_id
----------------------------------column 19---------------------------
left join
(select * from
(select location_id,scheme_id,rcc_cost as col19 from prwss_main.pmm_dsr_tubwell
union all
select location_id,scheme_id,rcc_cost as col19 from prwss_main.pmm_dsr_canal)x)i
on main.scheme_id=i.scheme_id
--------------------------------column 20------------------------------
left join
(select * from
(select location_id,scheme_id,(pump_total_cost+distribution_total_cost) as col20 from prwss_main.pmm_dsr_tubwell
union all
select location_id,scheme_id,(pump_cost+distribution_cost) as col20 from prwss_main.pmm_dsr_canal)x)j
on main.scheme_id=j.scheme_id
-------------------------------column 21-------------------------------
left join
(select * from
(select location_id,scheme_id,(cost_of_electric_connection+cost_of_ci_fitting+vnotch_cost+disinfection_cost+electronic_cost+plinth_cost+rising_cost+total_cost_topographical+
	total_cost_development_water+bulk_total_cost+slueice_cost+total_cost_structure+percapita_cost+environment_cost+grand_total) as  col21 from prwss_main.pmm_dsr_tubwell
union all
select location_id,scheme_id,(disinfection_cost+electronic_cost+plinth_cost+pipline_cost+total_cost_site_laying+slueice_cost+cost_of_electric_connection_provision+
	total_cost_of_topographical_survey+storage_cost+suction_cost+high_cost+filter_cost+clear_cost+bulk_total_cost+inlet_total_cost+pumping_machinery_cost+
	percapita_cost+environment_cost+contigency_charges+grand_total) as  col21  from prwss_main.pmm_dsr_canal)x)k
on main.scheme_id=k.scheme_id)xxxx
where $P!{where}
order by zone_name,circle_name,district_name,block_name,swap_nonswap,vill_name]]>
	</queryString>
