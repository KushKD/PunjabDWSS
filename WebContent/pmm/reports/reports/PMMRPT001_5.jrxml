	<queryString>
		<![CDATA[select * from
(select *,case when svd_swap_nonswap='SWAP' and program_id='PROG15' then  'SWAP-IDA'
	    when svd_swap_nonswap='SWAP' and program_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'
	    else svd_swap_nonswap end swap_nonswap from
(select  main.*,
	case when nc_pc_status='PC' and no_of_water_connection_before_comm <>0 then no_of_water_connection_before_comm else 0 end as connection_before,
	case when nc_pc_status='PC' and no_of_standpost_before_comm <>0 then no_of_standpost_before_comm else 0 end as stand_post_before,
	case when connection_per_pre >= 70 and connection_per_pre < 100 then (village_name) end  as per_70_99_conn,
	case when connection_per_pre < 70  then (village_name) end  as per_0_70_conn,
	case when connection_per_pre >= 100 then (village_name) end  as per_100_conn

from
(select  z.zone_id,z.zone_name,c.circle_id,c.circle_name,d.district_id,d.district_name,b.block_id,b.block_name,svd.scheme_id,svd.scheme_name,
svd.village_id,mv.village_name,svd.habitation_type,total_population,total_households,scheme_commissioned_date,scheme_operated_by,svd.swap_nonswap as svd_swap_nonswap,svd.program_id,

rank() over (partition by mv.village_id order by scheme_commissioned_date desc, scheme_id ) row_sch,

no_of_water_connection_after_comm,no_of_standpost_after_comm,no_of_water_connection_before_comm,no_of_standpost_before_comm,

case when total_households<>0 then no_of_water_connection_after_comm*100/total_households else 0 end as connection_per_pre,


--------------------------------

nc_pc.nc_pc_status,svd.name_of_gram_panchayat


from  (select * from (select sh.district_id,sh.scheme_id,sh.prog_id as program_id,scheme_name,prog_id,scheme_commissioned_date,scheme_operated_by,mv.village_id,sh.scheme_source,habitation_type,
	nc_pc_status,case when  program_id='PROG15' then  'SWAP-IDA'
	    when  program_id in ('PROG06','PROG11','PROG20') then 'SWAP-NON IDA'
	    else 'NONSWAP' end swap_nonswap,name_of_gram_panchayat,
	rank() over (partition by mv.village_id order by scheme_commissioned_date desc,sh.scheme_id desc,admin_app_gop_letter_date desc,sh.scheme_upgraded) rank
	from prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd,prwss_main.mst_village mv,
	prwss_main.tbl_proposed_program_2008 ncpc,prwss_main.mst_program p
	where sv.scheme_id=sh.scheme_id and sh.scheme_id=sd.scheme_id and sv.village_id=mv.village_id --and sh.location_id=mv.divisional_office_id
	and   mv.village_id=ncpc.village_id and sh.prog_id=p.program_id and sh.scheme_source not in ('SEWERAGE','HANDPUMP')
	and sv.status<>'D' and sh.status<>'D' and sd.status<>'D' and mv.status<>'D' and habitation_type='MH'
	and	  sv.scheme_upgraded not like 'ext%'
	and	  sd.scheme_upgraded not like 'ext%'
	and	  sh.scheme_upgraded not like 'ext%'
	and   scheme_commissioned_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd')
	)xx
	where rank=1 --and prog_id  in ('PROG15','PROG06','PROG11','PROG20')
)  svd

left outer join

(

select village_id,sum(total_population) total_population,sum(no_of_water_connection_after_comm) no_of_water_connection_after_comm,
sum(total_households) total_households,sum(no_of_standpost_after_comm)no_of_standpost_after_comm,sum(no_of_standpost_before_comm)no_of_standpost_before_comm,
sum(no_of_water_connection_before_comm) no_of_water_connection_before_comm
from

(---------------------------------population---------------------------------------------

select village_id,(gen_population+sc_population) as total_population,0 as no_of_water_connection_after_comm
,0 as total_households,
0 as no_of_standpost_after_comm,0 no_of_water_connection_before_comm,0 as  no_of_standpost_before_comm
from
(select *,rank() over (partition by village_id order by as_on_date desc,population_id desc) as row_id

From  prwss_main.mst_village_population mp

where as_on_date <=to_date($P{to_date},'yyyy-MM-dd')
and status<>'D'
order by village_id) main

where row_id=1



union all
---------------------------------connection after comm----------------------------------------

select distinct main.village_id,0 as total_population,
no_of_water_connection as no_of_water_connection_after_comm,0 as total_households,
no_of_standpost as  no_of_standpost_after_comm,0 no_of_water_connection_before_comm,0 as  no_of_standpost_before_comm
 from

(select mp.*,rank() over (partition by mp.village_id order by as_on_date desc,connection_id desc) as row_id

from prwss_main.mst_village_connection mp left outer join prwss_main.mst_scheme_village msv on mp.village_id=msv.village_id

where as_on_date < to_date($P{to_date},'yyyy-MM-dd')
and as_on_date >= msv.scheme_commissioned_date
and mp.status<>'D'
and msv.status<>'D'
order by mp.village_id) main
where row_id=1

union all
---------------------------------connection before comm----------------------------------------

select distinct main.village_id,0 as total_population,
0 as no_of_water_connection_after_comm,0 as total_households,
0 as  no_of_standpost_after_comm,no_of_water_connection as no_of_water_connection_before_comm,no_of_standpost as  no_of_standpost_before_comm
 from

(select mp.*,rank() over (partition by mp.village_id order by as_on_date asc,connection_id asc) as row_id

from prwss_main.mst_village_connection mp left outer join prwss_main.mst_scheme_village msv on mp.village_id=msv.village_id

where as_on_date < msv.scheme_commissioned_date
and msv.scheme_commissioned_date between to_date($P{from_date},'yyyy-MM-dd') and  to_date($P{to_date},'yyyy-MM-dd')
and mp.status<>'D'
and msv.status<>'D'
order by mp.village_id) main
where row_id=1


union all
---------------------------------household---------------------------------------------

select village_id,0 as total_population,0 as no_of_water_connection_after_comm,
(sc_households+gc_households)total_households,0 no_of_standpost_after_comm,0 no_of_water_connection_before_comm,0 as  no_of_standpost_before_comm
from

(select *,rank() over (partition by village_id order by as_on_date desc,household_id desc) as row_id

from prwss_main.mst_village_households mp

where as_on_date <= to_date($P{to_date},'yyyy-MM-dd')
and mp.status<>'D'
order by village_id) main

where row_id=1

) x

group by village_id

)y

on y.village_id=svd.village_id,prwss_main.mst_block b,prwss_main.mst_district d,prwss_main.mst_zone z,prwss_main.mst_circle c,prwss_main.mst_village mv,prwss_main.tbl_proposed_program_2008 nc_pc

where svd.district_id=d.district_id
and  mv.block_id=b.block_id
--and  b.district_id=d.district_id
and d.circle_id=c.circle_id
and c.zone_id=z.zone_id
and mv.village_id=svd.village_id
and svd.scheme_commissioned_date is not null
and svd.village_id=nc_pc.village_id


order by district_name
) main where row_sch=1 and $P!{nc_pc}) xx
--where (per_70_99_conn is not null or per_0_70_conn is not null)
)xxx
where $P!{where}
order by district_name asc]]>
	</queryString>
