	<queryString>
		<![CDATA[select * from
(select *
from
(select main.*,case when connection_per_pre >= 70 and connection_per_pre <100 then (village_name) end  as per_70_99_conn,
	      case when connection_per_pre >= 100 then (village_name) end  as per_100_conn
from
(select z.zone_id,z.zone_name,c.circle_id,c.circle_name,d.district_id,d.district_name,b.block_id,b.block_name,
  xx. scheme_id,
xx.scheme_name,
xx.village_id,xx.village_name,xx.habitation_type,total_population,total_households,scheme_commissioned_date,
no_of_water_connection_after_comm,no_of_standpost_after_comm,
case when total_households<>0 then no_of_water_connection_after_comm*100/total_households end as connection_per_pre,
nc_pc_status
from

(select svd.*,y.total_population,y.no_of_water_connection_after_comm,total_households,no_of_standpost_after_comm
from
(select * from
(select distinct location_id,sh.district_id,sh.scheme_id,scheme_name,prog_id as program_id,sh.scheme_source,water_works_existing_new,admin_app_gop_letter_no,admin_app_gop_letter_date,mv.village_id,
	scheme_commissioned_date,swap_nonswap,program_name,case when scheme_operated_by is not null and row_id=1 then scheme_operated_by else null end as scheme_operated_by,
	supply_service_level,case when  row_id=1  and (ro_plant=TRUE or ro_plant=FALSE) then ro_plant else null  end as ro_plant,row_id,
	rank() over (partition by mv.village_id order by scheme_commissioned_date desc,sh.scheme_id desc,admin_app_gop_letter_date desc,water_works_existing_new) as rank,
	case when p.swap_nonswap='SWAP' and program_id='PROG15' then  'SWAP-IDA'
	     when p.swap_nonswap='SWAP' and program_id<>'PROG15' then 'SWAP-NON IDA'
	     else p.swap_nonswap end swapnonswap,habitation_type,block_id,village_name,ncpc.nc_pc_status
from prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd,prwss_main.mst_program p,
(
select distinct scheme_id,village_id,scheme_commissioned_date,scheme_operated_by,ro_plant,supply_service_level,row_id from
(select sv.*,rank() over (partition by village_id order by scheme_commissioned_date desc,sh.scheme_id desc,admin_app_gop_letter_date desc) as row_id
from prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_details sd
where sh.scheme_id=sv.scheme_id and sv.scheme_id=sd.scheme_id and sh.status<>'D' and sv.status<>'D' and sd.status<>'D'
and   sh.scheme_source not in ('SEWERAGE','HANDPUMP')
and	  sv.scheme_upgraded not like 'ext%'
and	  sd.scheme_upgraded not like 'ext%'
and	  sh.scheme_upgraded not like 'ext%'
and   scheme_commissioned_date <=to_date($P{to_date},'yyyy-MM-dd')
order by village_id) con
where row_id in (1)
)sv,prwss_main.mst_village mv,prwss_main.tbl_proposed_program_2008 ncpc
where sh.scheme_id=sd.scheme_id
and sd.scheme_id=sv.scheme_id
and sv.village_id=mv.village_id
and mv.village_id=ncpc.village_id
and sh.prog_id=p.program_id
and habitation_type='MH'
and sh.status<>'D'
and sd.status<>'D' and	  sd.scheme_upgraded not like 'ext%'
and	  sh.scheme_upgraded not like 'ext%' )abc where rank=1)svd,
--prwss_main.vw_scheme_village_details_14_sep_2011 svd

--left join

(

select village_id,sum(total_population) total_population,sum(no_of_water_connection_after_comm) no_of_water_connection_after_comm,
sum(total_households) total_households,sum(no_of_standpost_after_comm)no_of_standpost_after_comm
from

(---------------------------------population---------------------------------------------

select village_id,(gen_population+sc_population) as total_population,0 as no_of_water_connection_after_comm
,0 as total_households,
0 as no_of_standpost_after_comm
from
(select *,rank() over (partition by village_id order by as_on_date desc,population_id desc) as row_id

From  prwss_main.mst_village_population mp

where as_on_date <=to_date($P{to_date},'yyyy-MM-dd')
and status<>'D'
order by village_id) main

where row_id=1



union all
---------------------------------connection after comm----------------------------------------

select distinct main.village_id,0 as total_population,
no_of_water_connection as no_of_water_connection_after_comm,0 as total_households,
no_of_standpost as  no_of_standpost_after_comm
 from

(select mp.*,rank() over (partition by mp.village_id order by as_on_date desc,connection_id desc,scheme_id) as row_id

from prwss_main.mst_village_connection mp left outer join prwss_main.mst_scheme_village msv on mp.village_id=msv.village_id

where as_on_date < to_date($P{to_date},'yyyy-MM-dd')
and as_on_date >= msv.scheme_commissioned_date
and mp.status<>'D'
and msv.status<>'D'
order by mp.village_id) main
where row_id=1

union all

---------------------------------household---------------------------------------------

select village_id,0 as total_population,0 as no_of_water_connection_after_comm,
(sc_households+gc_households)total_households,0 no_of_standpost_after_comm  from

(select *,rank() over (partition by village_id order by as_on_date desc,household_id desc) as row_id

from prwss_main.mst_village_households mp

where as_on_date <= to_date($P{to_date},'yyyy-MM-dd')
and mp.status<>'D'
order by village_id) main

where row_id=1

) x

group by village_id

)y

where svd.village_id=y.village_id)xx,prwss_main.mst_block b,prwss_main.mst_district d,prwss_main.mst_zone z,prwss_main.mst_circle c
where xx.block_id=b.block_id
and   b.district_id=d.district_id
and   d.circle_id=c.circle_id
and   c.zone_id=z.zone_id)main)xxx
where (per_70_99_conn is not null or per_100_conn is not null))final
where $P!{where}
order by zone_name,circle_name,district_name,block_name]]>
