	<queryString>
		<![CDATA[select * from (
select  z.zone_id,z.zone_name,c.circle_id,c.circle_name,d.district_id,d.district_name,b.block_id,b.block_name,dv.divisional_id,dv.divisional_name,
mc.constituency_id,mc.constituency_name,x.village_id,x.village_name,x.nc_pc_status as present_status,
EXTRACT(month from to_date('$P!{to_date}','yyyy-mm-dd'))||'/'||EXTRACT(year from to_date('$P!{to_date}','yyyy-mm-dd')) mnth_yr,
case when xcom.prog_id is not null then xcom.swap_nonswap
when xcom.prog_id is null and x.nc_pc_status='FC' then 'NA'
when xcom.prog_id is null and (x.nc_pc_status='PC' or x.nc_pc_status='NC') then pro.proposed_swap_nonswap end cover_under,

case when xcom.prog_id is null and x.nc_pc_status='FC' then 'NA'
when xcom.prog_id is null and (x.nc_pc_status='PC' or x.nc_pc_status='NC') and pro.proposed_program_id='PROG15' then 'PRWSS Project'
when xcom.prog_id is null and (x.nc_pc_status='PC' or x.nc_pc_status='NC') and pro.proposed_program_id<>'PROG15' then pro.proposed_program_name
when xcom.prog_id is not null
	and scheme_commissioned_date is null
	and (x.nc_pc_status='PC' or x.nc_pc_status='NC')
	and admin_app_gop_letter_no is not null
	and admin_app_gop_letter_date>='2008-04-01'  then xcom.program_name
when xcom.prog_id is not null
	and scheme_commissioned_date is null
	and (x.nc_pc_status='PC' or x.nc_pc_status='NC')
	and (admin_app_gop_letter_no is null or admin_app_gop_letter_date is null)
	and pro.proposed_program_id='PROG15'  then 'PRWSS Project'
when xcom.prog_id is not null
	and scheme_commissioned_date is null
	and (x.nc_pc_status='PC' or x.nc_pc_status='NC')
	and (admin_app_gop_letter_no is null or admin_app_gop_letter_date is null)
	and pro.proposed_program_id<>'PROG15'  then pro.proposed_program_name
when xcom.prog_id is not null
	and scheme_commissioned_date is null
	and (x.nc_pc_status='PC' or x.nc_pc_status='NC')
	and admin_app_gop_letter_no is not null
	and admin_app_gop_letter_date>='2008-04-01'  then xcom.program_name
when xcom.prog_id is not null
	and scheme_commissioned_date is null
	and (x.nc_pc_status='PC' or x.nc_pc_status='NC')
	and admin_app_gop_letter_no is not null
	and admin_app_gop_letter_date<'2008-04-01'  then 'PRWSS Project'
when xcom.prog_id is not null
	and scheme_commissioned_date is not null
	and (x.nc_pc_status='PC' or x.nc_pc_status='NC')
	and admin_app_gop_letter_no is not null
	and admin_app_gop_letter_date<'2008-04-01'  then 'PRWSS Project'
when xcom.prog_id is not null
	and scheme_commissioned_date is not null
	and (x.nc_pc_status='PC' or x.nc_pc_status='NC')
	and admin_app_gop_letter_no is not null
	and admin_app_gop_letter_date>='2008-04-01'  then xcom.program_name
when xcom.prog_id is not null then xcom.program_name
end name_of_prog,admin_app_gop_letter_no,admin_app_gop_letter_date,pro.proposed_program_name,
total_population as population,no_of_water_connection as water_connection,no_of_standpost,households,

case when (scheme_commissioned_date is null or scheme_commissioned_date<'2008-04-01')
	and (x.nc_pc_status='PC' or x.nc_pc_status='NC')
	and admin_app_gop_letter_date>'2008-04-01'
     then 'Under Progress'
     when  (scheme_commissioned_date is null or scheme_commissioned_date<'2008-04-01')
	and admin_app_gop_letter_date is null
	and (x.nc_pc_status='PC' or x.nc_pc_status='NC')
     then 'Yet to be taken up'
     when  (scheme_commissioned_date is null or scheme_commissioned_date<'2008-04-01')
	and admin_app_gop_letter_date <'2008-04-1'
	and (x.nc_pc_status='PC' or x.nc_pc_status='NC')
     then 'Yet to be taken up'

     when scheme_commissioned_date is not null
	and (x.nc_pc_status='PC' or x.nc_pc_status='NC')
	and scheme_commissioned_date>=to_date('2008-04-01','yyyy-mm-dd')
     then to_char(scheme_commissioned_date, 'MON/yyyy')
end as scheme_commissioned_date
 from (
select mv.*,nc_pc.nc_pc_status from prwss_main.mst_village mv, prwss_main.tbl_proposed_program_2008 nc_pc
where mv.village_id=nc_pc.village_id
and mv.habitation_type='MH'
and  mv.village_id in (select village_id from prwss_main.vw_prwss_project_habitation)
and mv.status<>'D'
) x
---------------------------------population---------------------------------------------
left outer join
(
select population_id,village_id,total_population,population_year,as_on_date from
(select *,rank() over (partition by village_id order by as_on_date desc,population_id desc) as row_id
from prwss_main.mst_village_population mp
where as_on_date <=to_date('$P!{to_date}','yyyy-MM-dd')
and status<>'D'
order by village_id) pop
where row_id=1
)xp on x.village_id=xp.village_id
---------------------------------pvt connection ---------------------------------------------
left outer join
(
select connection_id,village_id,no_of_water_connection,no_of_standpost,as_on_date from
(select *,rank() over (partition by village_id order by as_on_date desc,connection_id desc) as row_id
from prwss_main.mst_village_connection
where as_on_date <=to_date('$P!{to_date}','yyyy-MM-dd')
and status<>'D'
order by village_id) con
where row_id=1
)xpc on x.village_id=xpc.village_id


---------------------------------household ---------------------------------------------
left outer join
(
select household_id,village_id,(sc_households+gc_households) as households,as_on_date from
(select *,rank() over (partition by village_id order by as_on_date desc,household_id desc) as row_id
from prwss_main.mst_village_households
where as_on_date <=to_date('$P!{to_date}','yyyy-MM-dd')
and status<>'D'
order by village_id) con
where row_id=1
)xh on x.village_id=xh.village_id
---------------------------------commissioning ---------------------------------------------
left outer join
(
select * from
(select mv.district_id,district_name,block_name,sv.village_id,mv.village_name,sh.scheme_id,scheme_name,prog_id,program_name,scheme_commissioned_date,nc_pc_status,
	admin_app_gop_letter_date,admin_app_gop_letter_no,swap_nonswap,
	rank() over(partition by mv.village_id order by scheme_commissioned_date desc,sh.scheme_id) as row_id
from prwss_main.mst_scheme_header sh,prwss_main.mst_scheme_village sv,prwss_main.mst_scheme_details sd,prwss_main.mst_village mv,prwss_main.vw_prwss_project_habitation ncPc,
	prwss_main.mst_program p,prwss_main.mst_block mb,prwss_main.mst_district md
where sh.scheme_id=sv.scheme_id
and   sv.scheme_id=sd.scheme_id
and   sv.village_id=mv.village_id
and   mv.block_id=mb.block_id
and   mb.district_id=md.district_id
and   mv.village_id=ncPc.village_id
and   sh.prog_id=p.program_id
and   sh.scheme_source not in ('SEWERAGE')
and   sh.status<>'D' and sv.status<>'D' and sd.status<>'D' and mv.status<>'D')xx where row_id=1 and prog_id in ('PROG06','PROG15','PROG11','PROG20'))xcom
 on x.village_id=xcom.village_id
, prwss_main.mst_zone z,prwss_main.mst_circle c, prwss_main.mst_district d, prwss_main.mst_divisional dv,prwss_main.mst_block b,prwss_main.mst_constituency mc,prwss_main.tbl_proposed_program_2008 pro
where z.zone_id=c.zone_id
and c.circle_id=d.circle_id
and d.district_id=b.district_id
and x.divisional_office_id=dv.divisional_id
and x.block_id=b.block_id
and x.constituency_id=mc.constituency_id
and x.village_id=pro.village_id
and x.village_id in (select village_id from prwss_main.vw_prwss_project_habitation)
) main
where $P!{where}
order by zone_name,circle_name,district_name,block_name,village_name asc, scheme_commissioned_date asc]]>
	</queryString>
