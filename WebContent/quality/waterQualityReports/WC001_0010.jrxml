	<queryString>
		<![CDATA[SELECT	*,substring(faild_param_name1,3) faild_param_name,
	case when faild_param >0 then 'Fail' else 'Pass' end as Status ,
Case when cast(Al as text)='-1' then 'NT' when cast(Al as text)='-2' then 'ND'when cast(Al as text) ='-3' then 'BDL' else cast(Al as text) end as aluminium_val,
Case when cast(Pb as text)='-1' then 'NT' when cast(Pb as text)='-2' then 'ND'when cast(Pb as text) ='-3' then 'BDL' else cast(Al as text) end as lead_val,
Case when cast(Se as text)='-1' then 'NT' when cast(Se as text)='-2' then 'ND'when cast(Se as text) ='-3' then 'BDL' else cast(Se as text) end as selenium_val,
Case when cast(Cr as text)='-1' then 'NT' when cast(Cr as text)='-2' then 'ND'when cast(Cr as text) ='-3' then 'BDL' else cast(Cr as text) end as chromium_val,
Case when cast(Hg as text)='-1' then 'NT' when cast(Hg as text)='-2' then 'ND'when cast(Hg as text) ='-3' then 'BDL' else cast(Hg as text) end as mercury_val,
Case when cast(Asr as text)='-1' then 'NT' when cast(Asr as text)='-2' then 'ND'when cast(Asr as text) ='-3' then 'BDL' else cast(Asr as text) end as arsenic_val,
Case when cast(Cd as text)='-1' then 'NT' when cast(Cd as text)='-2' then 'ND'when cast(Cd as text) ='-3' then 'BDL' else cast(Cd as text) end as cadmium_val,
Case when cast(Ni as text)='-1' then 'NT' when cast(Ni as text)='-2' then 'ND'when cast(Ni as text) ='-3' then 'BDL' else cast(Ni as text) end as nickel_val,
Case when cast(TDS as text)='-1' then 'NT' when cast(TDS as text)='-2' then 'ND'when cast(TDS as text) ='-3' then 'BDL' else cast(TDS as text) end as TDS_val,
Case when cast(Fe as text)='-1' then 'NT' when cast(Fe as text)='-2' then 'ND'when cast(Fe as text) ='-3' then 'BDL' else cast(Fe as text) end as iron_val,
Case when cast(Cu as text)='-1' then 'NT' when cast(Cu as text)='-2' then 'ND'when cast(Cu as text) ='-3' then 'BDL' else cast(Cu as text) end as copper_val,
Case when cast(U as text)='-1' then 'NT' when cast(U as text)='-2' then 'ND'when cast(U as text) ='-3' then 'BDL' else cast(U as text) end as uranium_val,
Case when cast(Cl as text)='-1' then 'NT' when cast(Cl as text)='-2' then 'ND'when cast(Cl as text) ='-3' then 'BDL' else cast(Cl as text) end as Chloride_val,
Case when cast(F as text)='-1' then 'NT' when cast(F as text)='-2' then 'ND'when cast(F as text) ='-3' then 'BDL' else cast(F as text) end as Fluoride_val,
Case when cast(NO3 as text)='-1' then 'NT' when cast(NO3 as text)='-2' then 'ND'when cast(NO3 as text) ='-3' then 'BDL' else cast(NO3 as text) end as nitrate_val,
Case when cast(SO4 as text)='-1' then 'NT' when cast(SO4 as text)='-2' then 'ND'when cast(SO4 as text) ='-3' then 'BDL' else cast(SO4 as text) end as sulphate_val,
Case when cast(Ca as text)='-1' then 'NT' when cast(Ca as text)='-2' then 'ND'when cast(Ca as text) ='-3' then 'BDL' else cast(Ca as text) end as calcium_val,
Case when cast(Mg as text)='-1' then 'NT' when cast(Mg as text)='-2' then 'ND'when cast(Mg as text) ='-3' then 'BDL' else cast(Mg as text) end as magnesium_val,
Case when cast(Col as text)='-1' then 'NT' when cast(Col as text)='-2' then 'ND'when cast(Col as text) ='-3' then 'BDL' else cast(Col as text) end as color_val,
Case when cast(Trb as text)='-1' then 'NT' when cast(Trb as text)='-2' then 'ND'when cast(Trb as text) ='-3' then 'BDL' else cast(Trb as text) end as turbidity_val,

Case when cast(pH as text)='-1' then 'NT' when cast(pH as text)='-2' then 'ND'when cast(pH as text) ='-3' then 'BDL' else cast(pH as text) end as pH_val,
Case when cast(TH as text)='-1' then 'NT' when cast(TH as text)='-2' then 'ND'when cast(TH as text) ='-3' then 'BDL' else cast(TH as text) end as hardness_val,
Case when cast(TA as text)='-1' then 'NT' when cast(TA as text)='-2' then 'ND'when cast(TA as text) ='-3' then 'BDL' else cast(TA as text) end as alkalinity_val
FROM	(
SELECT	BB.*,scheme_name,village_name,block_id,block_name,district_id,district_name,divisional_id,divisional_name,
	subdiv_id,subdiv_name,constituency_id,constituency_name,circle_id,circle_name,zone_id,zone_name,
CASE WHEN Al not in(-1,-2,-3) and (Al>0.2) then ' ,aluminium'  ELSE '' END||CASE WHEN Pb not in(-1,-2,-3) and (Pb>0.05) then ' ,lead' ELSE '' END||
CASE WHEN Se not in(-1,-2,-3) and (Se>0.01) then ' ,selenium' ELSE '' END||CASE WHEN Cr not in(-1,-2,-3) and (Cr>0.05) then ' ,chromium' ELSE '' END||
CASE WHEN Hg not in(-1,-2,-3) and (Hg>0.001) then ' ,mercury' ELSE '' END||CASE WHEN Asr not in(-1,-2,-3) and (Asr>0.01) then ' ,arsenic' ELSE '' END||
CASE WHEN Cd not in(-1,-2,-3) and (Cd>0.03) then ' ,cadmium' ELSE '' END||CASE WHEN Ni not in(-1,-2,-3) and (Ni>0.02) then ' ,nickel' ELSE '' END||
CASE WHEN TDS not in(-1,-2,-3) and (TDS>2000.00) then ' ,TDS' ELSE '' END||CASE WHEN  Fe not in(-1,-2,-3) and ( Fe>1.0) then ' ,iron' ELSE '' END||
CASE WHEN Cu not in(-1,-2,-3) and (Cu>1.5) then ' ,copper' ELSE '' END||CASE WHEN U not in(-1,-2,-3) and (U>60.0) then ' ,uranium' ELSE '' END||
CASE WHEN F not in(-1,-2,-3) and (F>1.5) then ' ,Fluoride' ELSE '' END||CASE WHEN Cl not in(-1,-2,-3) and (Cl>1000.00) then ' ,Chloride' ELSE '' END||
CASE WHEN NO3 not in(-1,-2,-3) and (NO3>45.0) then ' ,nitrate' ELSE '' END||CASE WHEN SO4 not in(-1,-2,-3) and (SO4>400.0) then ' ,sulphate' ELSE '' END||
CASE WHEN Ca not in(-1,-2,-3) and (Ca>200.0) then ' ,calcium' ELSE '' END||CASE WHEN Mg not in(-1,-2,-3) and (Mg>100.0) then ' ,magnesium' ELSE '' END||
CASE WHEN Col not in(-1,-2,-3) and (Col>15.00) then ' ,Colour' ELSE '' END||CASE WHEN  Trb not in(-1,-2,-3) and ( Trb>5.0) then ' ,turbidity' ELSE '' END||
CASE WHEN pH not in(-1,-2,-3) and (pH>8.5) then ' ,ph' ELSE '' END||CASE WHEN TH not in(-1,-2,-3) and (TH>600.0) then ' ,hardness' ELSE '' END||
CASE WHEN TA not in(-1,-2,-3) and (TA>5.0) then ' ,alkalinity' ELSE '' END AS FAILD_PARAM_NAME1,
CASE WHEN Al not in(-1,-2,-3) and (Al>0.2) then   1 else 0 END+	CASE WHEN Pb not in(-1,-2,-3) and (Pb>0.05) then  1 else 0 END+
CASE WHEN Se not in(-1,-2,-3) and (Se>0.01) then 1 else 0 END+CASE WHEN Cr not in(-1,-2,-3) and (Cr>0.05) then  1 else 0 END+
CASE WHEN Hg not in(-1,-2,-3) and (Hg>0.001) then  1 else 0 END+CASE WHEN Asr not in(-1,-2,-3) and (Asr>0.01) then  1 else 0 END+
CASE WHEN Cd not in(-1,-2,-3) and (Cd>0.03) then  1 else 0 END+	CASE WHEN Ni not in(-1,-2,-3) and (Ni>0.02) then 1 else 0 END+
CASE WHEN TDS not in(-1,-2,-3) and (TDS>2000.00) then 1 else 0 END+	CASE WHEN  Fe not in(-1,-2,-3) and ( Fe>1.0) then 1 else 0 END+
CASE WHEN Cu not in(-1,-2,-3) and (Cu>1.5) then  1 else 0 END+CASE WHEN(U not in(-1,-2,-3)) and (U>60.0) then  1 else 0 END+
CASE WHEN F not in(-1,-2,-3) and (F>1.5) then   1 else 0 END+CASE WHEN Cl not in(-1,-2,-3) and (Cl>1000.00) then   1 else 0 END+
CASE WHEN NO3 not in(-1,-2,-3) and (NO3>45.0) then  1 else 0 END+CASE WHEN SO4 not in(-1,-2,-3) and (SO4>400.0) then  1 else 0 END+
CASE WHEN Ca not in(-1,-2,-3) and (Ca>200.0) then  1 else 0 END+CASE WHEN Mg not in(-1,-2,-3) and (Mg>100.0) then  1 else 0 END+
CASE WHEN Col not in(-1,-2,-3) and (Col>15.00) then  1 else 0 END+CASE WHEN  Trb not in(-1,-2,-3) and ( Trb>5.0) then  1 else 0 END
	AS FAILD_PARAM FROM(
SELECT	village_id,scheme_id,sample_id,sample_number,crt_date as date_of_receipt,water_source,
	sum(case when param_id=1 then actual_value end) as Col,
	sum(case when param_id=4 then actual_value end) as Trb,
	sum(case when param_id=5 then actual_value end) as pH,
	sum(case when param_id=6 then actual_value end) as TDS,
	sum(case when param_id=7 then actual_value end) as TH,
	sum(case when param_id=8 then actual_value end) as TA,
	sum(case when param_id=9 then actual_value end) as Fe,
	sum(case when param_id=10 then actual_value end) as Cl,
	sum(case when param_id=12 then actual_value end) as SO4,
	sum(case when param_id=14 then actual_value end) as NO3,
	sum(case when param_id=15 then actual_value end) as F,
	sum(case when param_id=16 then actual_value end) as Ca,
	sum(case when param_id=17 then actual_value end) as Cu,
	sum(case when param_id=18 then actual_value end) as Mg,
	sum(case when param_id=20 then actual_value end) as Hg,
	sum(case when param_id=21 then actual_value end) as Cd,
	sum(case when param_id=22 then actual_value end) as Se,
	sum(case when param_id=23 then actual_value end) as Asr,
	sum(case when param_id=25 then actual_value end) as Pb,
	sum(case when param_id=27 then actual_value end) as Cr,
	sum(case when param_id=28 then actual_value end) as Al,
	sum(case when param_id=34 then actual_value end) as Ni,
	sum(case when param_id=46 then actual_value end) as U
FROM	(SELECT	village_id ,scheme_id,aa.test_result_id,sc.sample_number,sc.crt_date,water_source,
	tr.sample_id,param_id,cast(actual_value as numeric) as actual_value
FROM	prwss_main.wq_sample_collection sc,	prwss_main.wq_test_result tr,
	prwss_main.wq_test_result_dtl aa
WHERE	tr.sample_id=sc.sample_id	and tr.test_result_id=aa.test_result_id
	and $P!{sample}---sample_number in ('BTLSDTL/1819/00005','TWESWTL/1819/00001')
)aa group by village_id,scheme_id,sample_id,sample_number,crt_date,water_source)BB,prwss_main.pms_scheme_dtl SCH,
(SELECT	vil.location_id AS village_id,		vil.location_name AS village_name,
	blo.location_id AS block_id,		blo.location_name AS Block_name,
	dis.location_id AS district_id,		dis.location_name AS district_name,
	div.div_subdiv_id AS Divisional_id,	div.div_subdiv_name AS Divisional_name,
	subdiv.div_subdiv_id AS subdiv_id,	subdiv.div_subdiv_name AS subdiv_name,
	con.constituency_id AS constituency_id,	constituency_name,
	cir1.location_id AS circle_id,		cir1.location_name AS circle_name,
	zon1.location_id AS zone_id,		zon1.location_name AS zone_name
FROM	prwss_main.village_detail_mapping map,	prwss_main.location_details vil,
	prwss_main.location_details blo,	prwss_main.location_details dis,
	prwss_main.location_div_subdiv_details div,
	prwss_main.location_div_subdiv_details subdiv,
	prwss_main.location_constituency_details con,
	prwss_main.location_master cir,		prwss_main.location_details cir1,
	prwss_main.location_master zon,		prwss_main.location_details zon1
WHERE	map.location_id=vil.location_id 	AND cast(map.block_id AS integer)=blo.location_id
	AND map.distric_id=dis.location_id 	AND map.divisional_id=div.div_subdiv_id
	AND map.sub_div=subdiv.div_subdiv_id 	AND map.constituancy_id=con.constituency_id
	AND dis.location_id=cir.location_id	AND cir.parent_location=cir1.location_id
	AND cir1.location_id=zon.location_id	AND zon.parent_location=zon1.location_id)ZZ
WHERE	BB.village_id=ZZ.village_id		AND BB.scheme_id=SCH.scheme_id	)Main

ORDER BY zone_name,circle_name,district_name,constituency_name,Divisional_name,block_name,subdiv_name,
village_name]]>
	</queryString>
